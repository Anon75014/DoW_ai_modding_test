class 'DarkEldarVenomTactic' (DarkEldarVehicleTactic)

function DarkEldarVenomTactic:__init( squad_ai ) super( squad_ai )

	self:SetName("Dark Eldar Venom Tactic")
	
	-- Vehicle is a light transport
	self.m_iTransportVehicle = 2
	self.m_iTransportSlots = 1
end

function DarkEldarVenomTactic:DoAbilities()

	-- New code to unstuck units with pathing issues, that can jump.
	-- Call it with [true], only for multi-membered squads.
	self:SolveStuckCase(false)
end