function kBuildBaseStrategy:ChooseBuildProgram()
	if (table.getn(self.info.BuildPrograms) ~= 4) then
		return BuildBaseStrategy.ChooseBuildProgram(self)
	end
	local sMapSize, iClosestEnemyDistance = self:GetMapSize()
	local iBuildProgram1
	local iBuildProgram2
	local iBuildProgram3
	local iBuildProgram4
	if (sMapSize == "small" or iClosestEnemyDistance <= 70 or cpu_manager.iPlayerCount == 2) then
		iBuildProgram1 = 40
		iBuildProgram2 = 30
		iBuildProgram3 = 30
		iBuildProgram4 = 0
	elseif (sMapSize == "large" and cpu_manager.iPlayerCount >= 6) then
		iBuildProgram1 = 40
		iBuildProgram2 = 20
		iBuildProgram3 = 20
		iBuildProgram4 = 20
	else
		iBuildProgram1 = 40
		iBuildProgram2 = 30
		iBuildProgram3 = 30
		iBuildProgram4 = 0
	end
	local oEnemy = cpu_manager:FindClosestEnemyPlayer(false)
	local sEnemy = oEnemy:GetPlayerRaceName()		
	if (sEnemy == "ork_race") then
		iBuildProgram2 = iBuildProgram2 + 10
		iBuildProgram3 = iBuildProgram3 - 10
	elseif (sEnemy == "eldar_race" or sEnemy == "guard_race" or sEnemy == "tau_race" or sEnemy == "necron_race") then
		iBuildProgram1 = iBuildProgram1 - 10
		iBuildProgram2 = iBuildProgram2 - 10
		iBuildProgram3 = iBuildProgram3 + 20
	end
	iBuildProgram2 = iBuildProgram1 + iBuildProgram2
	iBuildProgram3 = iBuildProgram2 + iBuildProgram3
	iBuildProgram4 = iBuildProgram3 + iBuildProgram4
	local iRandom = math.random(1, 100)
	if (iRandom <= iBuildProgram1) then
		return 1
	elseif (iRandom <= iBuildProgram2) then
		return 2
	elseif (iRandom <= iBuildProgram3) then
		return 3
	elseif (iRandom <= iBuildProgram4) then
		return 4
	end
	return 1
end

function kBuildBaseStrategy:EvaluateSquadCap()
	if (self:CheckSquadCap(100, 0)) then
		local tBuildType = CpuBuildType()
		tBuildType.btype = CpuPrerequisites.BT_Research
		if (not cpu_manager.cpu_player:IsResearchComplete( "khm_squad_cap_research" )) then
			tBuildType.name = "khm_squad_cap_research"
		elseif not cpu_manager.cpu_player:IsResearchComplete( "khm_squad_cap_research_1" ) then
			tBuildType.name = "khm_squad_cap_research_1"
		elseif (not cpu_manager.cpu_player:IsResearchComplete( "khm_squad_cap_research_2" )) then
			tBuildType.name = "khm_squad_cap_research_2"
		elseif (not cpu_manager.cpu_player:IsResearchComplete( "khm_squad_cap_research_3" )) then
			tBuildType.name = "khm_squad_cap_research_3"
		elseif (not cpu_manager.cpu_player:IsResearchComplete( "khm_support_cap_research" )) then
			tBuildType.name = "khm_support_cap_research"
		elseif (not cpu_manager.cpu_player:IsResearchComplete( "khm_support_cap_research_1" )) then
			tBuildType.name = "khm_support_cap_research_1"
		else
			return
		end
		if (self:TryBuild( tBuildType )) then
			aitrace("BuildController: Dynamic research of "..tBuildType.name)
		end
	end
end

function kBuildBaseStrategy:GetBuildingName( sType )
	if (sType == "HQ") then
		return "khm_hq"
	elseif (sType == "Generator") then
		return "khm_plasma_generator"
	elseif (sType == "BiggerGenerator") then
		return "khm_thermo_plasma_generator"
	elseif (sType == "VehicleBuilding") then
		return "khm_machine_pit"
	elseif (sType == "ListeningPost") then
		return "khm_listening_post"
	elseif (sType == "Turret") then
		return "khm_turret"
	elseif (sType == "Mine") then
		return "khm_razor_mine"
	end
	return nil
end

