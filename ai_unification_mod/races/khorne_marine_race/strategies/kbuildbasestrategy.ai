function kBuildBaseStrategy:UpdateTierLevel()
	self.tierLevel = 1
	if (self:GetBuildingCountByName("khm_armoury") > 0) then
		self.tierLevel = 2
	end
	local iHQAddon2ID = cpu_manager.stats:GetAddOnID("khm_hq_addon")
	local oStats = cpu_manager.stats:GetPlayerStatsFromID( cpu_manager.player_id )
	for oBase in oStats:GetBases() do
		if (oBase:IsValid() and not oBase:IsListeningPost()) then
			if (oBase:HasAddOn(iHQAddon2ID)) then
				if (cpu_manager.cpu_player:IsResearchComplete("khm_blood_crusade_research")) then
					self.tierLevel = 4
					return
				else
					self.tierLevel = 3
					return
				end
			end
		end
	end
end

function kBuildBaseStrategy:BuildFlexible()

	-- Always test first to see if there is at least one existing HQ
	if self:GetBuildingCountByName(self:GetBuildingName("HQ")) < 1 then
     		self:BuildFirstHQ()
	end

	-- Always test first to see if there is at least one existing builder unit
	self:BuildFirstBuilder("khm_squad_slave")

	-- RAGE Last Standing Hero Wargear
	local iHeroLSCommanderSquads = self:CountSquads("khm_squad_hero_kharn_ktgm")
	if iHeroLSCommanderSquads > 0 then
		self:DynamicResearch("khm_ls_hero_kharn_commander_health_research_1", 1, 50, 50, 0, 0, nil, 0)
		self:DynamicResearch("khm_ls_hero_kharn_commander_health_research_2", 1, 75, 75, 0, 0, nil, 0)

		if not G_KTGM_Commander_WG_Choice_Made then
			 -- WARNING: Below, ALWAYS insert numbers <= of the total in each table!
           		 -- I used the actual total of each table, so that ALL will be randomly available
            		i_WG_Selection_number1 = 10	-- The number of researches to randomize, from the 1st table
            		i_WG_Selection_number2 = 8	-- The number of researches to randomize, from the 2nd table

            		i_WG_Selections1 = {
                	{"khm_ls_hero_kharn_accuracy_upgrade_research", 1, 125, 125, 0, 0, nil, 0},
                	{"khm_ls_hero_kharn_bfbg_research", 1, 150, 150, 0, 0, nil, 0},
                	{"khm_ls_hero_kharn_icon_meat_carrier_research", 1, 150, 150, 0, 0, nil, 0},
                	{"khm_ls_hero_kharn_krak_grenades_research", 1, 150, 150, 0, 0, nil, 0},
                	{"khm_ls_hero_kharn_melee_upgrade_research", 1, 100, 100, 0, 0, nil, 0},
                	{"khm_ls_hero_kharn_melee_upgrade_research_2", 1, 125, 125, 0, 0, nil, 0},
                	{"khm_ls_hero_kharn_regeneration_research", 1, 125, 125, 0, 0, nil, 0},
                	{"khm_ls_hero_kharn_speed_research", 1, 150, 150, 0, 0, nil, 0},
                	{"khm_ls_hero_kharn_the_smell_of_blood_research", 1, 150, 150, 0, 0, nil, 0},
                	{"khm_ls_hero_kharn_blood_letter_research", 1, 175, 175, 0, 0, nil, 0}}

            		i_WG_Selections2 = {
                	{"khm_ls_hero_kharn_blood_point", 1, 125, 125, 0, 0, nil, 0},
                	{"khm_ls_hero_kharn_commander_chosen", 1, 125, 125, 0, 0, nil, 0},
                	{"khm_ls_hero_kharn_commander_health_research_3", 1, 125, 125, 0, 0, nil, 0},
                	{"khm_ls_hero_kharn_furious_rage", 1, 125, 125, 0, 0, nil, 0},
                	{"khm_ls_hero_kharn_melta_bomb_research", 1, 125, 125, 0, 0, nil, 0},
                	{"khm_ls_hero_kharn_rally", 1, 125, 125, 0, 0, nil, 0},
                	{"khm_ls_hero_kharn_melee_aura", 1, 125, 125, 0, 0, nil, 0},
                	{"khm_ls_hero_kharn_symbol_of_chaos", 1, 125, 125, 0, 0, nil, 0}}

            		-- The switch ensures we make the choice only ONCE
            		G_KTGM_Commander_WG_Choice_Made = true
            		i_WG_Rdm1 = {}          -- The random Selections from the 1st table
            		i_WG_Rdm2 = {}          -- The random Selections from the 2nd table

            		-- Populate the two random tables with the number of researches we need for each
            		for i = 1, i_WG_Selection_number1 do
                		local selection = math.random(1, table.getn(i_WG_Selections1))
                		table.insert(i_WG_Rdm1, i_WG_Selections1[selection])
                		table.remove(i_WG_Selections1, selection)
            		end

            		for i = 1, i_WG_Selection_number2 do
                		local selection = math.random(1, table.getn(i_WG_Selections2))
                		table.insert(i_WG_Rdm2, i_WG_Selections2[selection])
                		table.remove(i_WG_Selections2, selection)
            		end
		end

       		-- We are done. Perform the Researches
        	for i = 1, table.getn(i_WG_Rdm1) do
            		self:DynamicResearch(i_WG_Rdm1[i][1], 0, i_WG_Rdm1[i][3], i_WG_Rdm1[i][4], 0, 0, nil, 0)
        	end

    		for i = 1, table.getn(i_WG_Rdm2) do
       			self:DynamicResearch(i_WG_Rdm2[i][1], 0, i_WG_Rdm2[i][3], i_WG_Rdm2[i][4], 0, 0, nil, 0)
       		end
	end

	local iRequisition = resource_manager:GetResourceAmount():Get( ResourceAmount.RT_Requisition )
	local iPower = resource_manager:GetResourceAmount():Get( ResourceAmount.RT_Power )
	local iArmyStrength = cpu_manager:GetArmyStrength()

	-- Compute Builder Pattern research
	if (self.m_iBuilderPattern == 1) then
		self:DynamicResearch("khm_technomancery_research", 1, 0, 0, 0, 0, nil, 0)
	elseif (self.m_iBuilderPattern == 2) then
		self:DynamicResearch("khm_daemonology_research", 1, 0, 0, 0, 0, nil, 0)
	end

	-- Compute tier 2 researches
	if (self.tierLevel >= 2) then

		if (iArmyStrength >= 250) then
			self:DynamicResearch("khm_frag_grenade_research", 2, 25, 75, 0, 0, nil, 0)		-- unlocks frags
		end
		if (cpu_manager.cpu_player:IsResearchComplete("khm_daemonology_research")) then
			self:DynamicResearch("khm_hellblade_research", 2, 150, 75, 0, 0, nil, 0)
		end
		-- Compute general researches
		if (iArmyStrength >= 550) then
			self:DynamicResearch("khm_the_smell_of_blood_research", 2, 75, 50, 0, 0, nil, 0)	-- keen sight increase for most squads and commanders
			self:DynamicResearch("khm_gift_research", 2, 200, 200, 0, 0, nil, 0)			-- req/power rate boost
			self:DynamicResearch("khm_furious_leap_research", 2, 75, 50, 0, 0, nil, 0)		-- enable melee leap
			self:DynamicResearch("khm_accuracy_upgrade_research", 2, 90, 55, 0, 0, nil, 0)		-- range damage increase for most standard squads
			self:DynamicResearch("khm_melee_upgrade_research", 2, 90, 55, 0, 0, nil, 0)		-- melee damage increase for most standard squads
			self:DynamicResearch("khm_special_upgrade_research", 2, 90, 55, 0, 0, nil, 0)		-- special weapons damage increase for most standard squads
			self:DynamicResearch("khm_bloodletters_research", 2, 75, 50, 0, 0, nil, 0)		-- more damage for bloodletters
			self:DynamicResearch("khm_bloodletters_research_2", 2, 75, 100, 0, 0, nil, 0)		-- more damage for bloodletters
			self:DynamicResearch("khm_jugg_research", 2, 175, 50, 0, 0, nil, 0)			-- unlocks juggernauts/terminators+research
			self:DynamicResearch("khm_health_upgrade_research", 2, 50, 30, 0, 0, nil, 0)		-- health increase for most standard squads
			self:DynamicResearch("khm_daemons_health_research", 2, 150, 80, 0, 0, nil, 0)		-- health increase for most daemon squads
			self:DynamicResearch("khm_commander_health_research_1", 2, 150, 50, 0, 0, nil, 0)	-- health increase for most commander squads
			self:DynamicResearch("khm_speed_research", 2, 75, 50, 0, 0, nil, 0)			-- speed increase for most squads and commanders
		end

		if (cpu_manager.cpu_player:IsResearchComplete("khm_frag_grenade_research")) then
			self:DynamicResearch("khm_krak_grenades_research", 2, 25, 50, 0, 0, nil, 0)
		end
		if (cpu_manager.cpu_player:IsResearchComplete("khm_krak_grenades_research")) then
			self:DynamicResearch("khm_melta_bomb_research", 2, 25, 50, 0, 0, nil, 0)
		end

		-- Try to build no-limit defensive turrets
		self:DynamicBuild("khm_turret_no_limit", 2, 2, 200, 100, 0, 0)

	end

	-- Compute tier 3 researches
	if (self.tierLevel >= 3) then

		if (self:CountSquads("khm_squad_defiler_blood") >= 1) then
			if (cpu_manager.cpu_player:IsResearchComplete("khm_daemonology_research")) then
			self:DynamicResearch("khm_defiler_improved_research", 3, 150, 300, 0, 0, nil, 0)
			end
		end
		-- Compute general researches
		if (iArmyStrength >= 750) then
			self:DynamicResearch("khm_melee_upgrade_research_2", 	3, 140, 75, 0, 0, nil, 0)	-- furthe rmelee damage increase for most standard squads
			self:DynamicResearch("khm_accuracy_upgrade_research_2", 3, 140, 75, 0, 0, nil, 0)	-- further range damage increase for most standard squads
			self:DynamicResearch("khm_special_upgrade_research_2", 	3, 140, 75, 0, 0, nil, 0)	-- further special weapons damage increase for most standard squads
			self:DynamicResearch("khm_health_upgrade_research_2", 	3, 100, 50, 0, 0, nil, 0)	-- further health increase for most standard squads
			self:DynamicResearch("khm_daemons_health_research_2", 	3, 250, 225, 0, 0, nil, 0)	-- further health increase for most daemon squads
			self:DynamicResearch("khm_commander_health_research_2", 3, 200, 100, 0, 0, nil, 0)	-- further health increase for most commander squads
			self:DynamicResearch("khm_gift_research_2", 3, 350, 350, 0, 0, nil, 0)			-- req/power rate boost
		end
		if (self:CountSquads("khm_squad_warpsmith") >= 1) then
			self:DynamicResearch("khm_harnesses_research", 2, 160, 50, 0, 0, nil, 0)
		end
		-- Compute Lord speciality
		if (self:CountSquads("khm_squad_lord_khorne") >= 1) then
			if (self.m_iLordResearchPattern == 1) then
				self:DynamicResearch("khm_assault_research", 3, 160, 50, 0, 0, nil, 0)
			elseif (self.m_iLordResearchPattern == 2) then
				self:DynamicResearch("khm_terminator_research", 3, 160, 50, 0, 0, nil, 0)
			elseif (self.m_iLordResearchPattern == 3) then
				self:DynamicResearch("khm_infernal_research", 3, 160, 50, 0, 0, nil, 0)
			end
		end
		if (self:CountSquads("khm_squad_cult_vehicles_meat_carrier") >= 1) then
			self:DynamicResearch("khm_smoke_launcher_research", 2, 75, 25, 0, 0, nil, 0)
			self:DynamicResearch("khm_icon_meat_carrier_research", 2, 75, 25, 0, 0, nil, 0)
		end

		-- Compute cap researches
		if (iArmyStrength >= 1250) then

			local iRequisition = resource_manager:GetResourceAmount():Get( ResourceAmount.RT_Requisition )
			local iPower = resource_manager:GetResourceAmount():Get( ResourceAmount.RT_Power )

			if (iRequisition > 300 and iPower > 100) then

				-- Compute Final Squad Cap Research due to not being researched in late game
				if (cpu_manager.cpu_player:IsResearchComplete("khm_squad_cap_research_2")) then
					self:DynamicResearch("khm_squad_cap_research_3", 3, 150, 0, 0, 0, nil, 0)
					--print("Build Squad Cap 3 for player "..tostring(cpu_manager.player_id).."")
				end
			end
		end

		-- Compute cap researches
		if (iArmyStrength >= 1750) then

			local iRequisition = resource_manager:GetResourceAmount():Get( ResourceAmount.RT_Requisition )
			local iPower = resource_manager:GetResourceAmount():Get( ResourceAmount.RT_Power )

			if (iRequisition > 400 and iPower > 200) then

				-- Compute Final Support Cap Research due to not being researched in late game
				if (cpu_manager.cpu_player:IsResearchComplete("khm_squad_cap_research_3")) then
					self:DynamicResearch("khm_support_cap_research", 3, 150, 0, 0, 0, nil, 0)
					--print("Build Support Cap 1 for player "..tostring(cpu_manager.player_id).."")
				end
			end
		end

		-- Force build additional generators in late game if we can with enough requisition and when low on power
                local iReq = resource_manager:GetResourceAmount():Get( ResourceAmount.RT_Requisition )
                local iPow = resource_manager:GetResourceAmount():Get( ResourceAmount.RT_Power )
                if iReq > 400 and iReq/iPow > 1.6 then

                	-- Check if a plan exists
                	local iBuildingID = cpu_manager.stats:GetBuildingID(self:GetBuildingName("Generator"))
                    	if (self:PlanExists("Build Building Plan", iBuildingID)) then
                       		return
                    	end

             		-- Check if any generators are in production
                    	for oBuildChannel in build_manager:GetBuildChannelAIs() do
                        	-- Check building ID
                        	if (oBuildChannel:GetBlueprintID() == iBuildingID and not oBuildChannel:ConstructionDone()) then
                            		return
                        	end
                    	end

             		-- Build a Generator
                    	local tBuildType = CpuBuildType()
                    	tBuildType.btype = CpuPrerequisites.BT_Building
                    	tBuildType.name = self:GetBuildingName("Generator")
                    	if (self:TryBuild( tBuildType )) then
               			aitrace("BuildController: Dynamic build of "..tBuildType.name)
			end
		end

		-- Try to build no-limit defensive turrets
		self:DynamicBuild("khm_turret_no_limit", 4, 3, 300, 200, 0, 0)
	end

	-- Compute tier 4 researches
	if (self.tierLevel >= 4) then

		-- Computer Titan researches
		self:DynamicResearch("khm_titan_research_1", 4, 450, 450, 0, 0, nil, 0)
		self:DynamicResearch("khm_titan_research_2", 4, 800, 800, 0, 0, nil, 0)

		-- Compute cap researches
		if (iArmyStrength >= 2250) then

			local iRequisition = resource_manager:GetResourceAmount():Get( ResourceAmount.RT_Requisition )
			local iPower = resource_manager:GetResourceAmount():Get( ResourceAmount.RT_Power )

			if (iRequisition > 500 and iPower > 300) then

				-- Compute Final Support Cap Research due to not being researched in late game
				if (cpu_manager.cpu_player:IsResearchComplete("khm_support_cap_research")) then
					self:DynamicResearch("khm_support_cap_research_1", 4, 150, 0, 0, 0, nil, 0)
					--print("Build Support Cap 2 for player "..tostring(cpu_manager.player_id).."")
				end
			end
		end

		-- Compute HQ Tier2 Addons for future HQs
		if self:GetBuildingCountByName("khm_hq") > 1 then
			self:DynamicAddon("khm_hq_addon", 100, 4, 340, 340, 0, 0, nil, nil, false)
		end

		-- Try to build no-limit defensive turrets
		self:DynamicBuild("khm_turret_no_limit", 10, 4, 400, 300, 0, 0)
	end

	-- Restrict dynamic builds to hard difficulty or higher
	if (CpuManager.AISettings.bMultiBuildings) then
		-- Item-Syntax: BuildingName, BuildingCount, MinTier, MinRequisition, MinPower, MinSquadCap, MinSupportCap
		self:DynamicBuild("khm_temple", 3, 2, 850, 400, 0, 0)
		self:DynamicBuild("khm_machine_pit", 3, 2, 1000, 500, 0, 0)
		self:DynamicBuild("khm_sacrificial_circle", 3, 2, 600, 300, 0, 0)
		self:DynamicBuild("khm_hq", 2, 3, 500, 400, 0, 0)
		self:DynamicBuild("khm_hq", 3, 4, 1000, 600, 0, 0)
	end
	self:BuildBanelord()
	self:BuildTechmarine()
	self:BuildDaemonhost()
	self:BuildHeroRandom()
end

-- Force AI to give priority to re-build HQ if lost and none are present
function kBuildBaseStrategy:BuildFirstHQ()

	-- Check if a plan exists
	local iBuildingID = cpu_manager.stats:GetBuildingID(self:GetBuildingName("HQ"))
	if (self:PlanExists("Build Building Plan", iBuildingID)) then
		return
	end

	-- Check if any HQ are in production
	for oBuildChannel in build_manager:GetBuildChannelAIs() do

		-- Check building ID
		if (oBuildChannel:GetBlueprintID() == iBuildingID and not oBuildChannel:ConstructionDone()) then
			return
		end
	end
		
	-- Build the HQ
	local sHQName = self:GetBuildingName("HQ")
	local tBuildType = CpuBuildType()
	tBuildType.btype = CpuPrerequisites.BT_Building
	tBuildType.name = sHQName
	if (self:TryBuild( tBuildType )) then
		aitrace("BuildController: Dynamic build of "..tBuildType.name)
	end
	return
end

-- Force AI to give priority to recruit a builder unit if none are present
function kBuildBaseStrategy:BuildFirstBuilder(iBuilderName)

	if self:CountSquads(iBuilderName) < 1 then

		-- Recruit a builder squad
		local tBuildType = CpuBuildType()
		tBuildType.btype = CpuPrerequisites.BT_Squad
		tBuildType.name = iBuilderName
		if (self:TryBuild( tBuildType )) then
			aitrace("BuildController: Dynamic build of "..tBuildType.name)
		end
		return
	end
end

