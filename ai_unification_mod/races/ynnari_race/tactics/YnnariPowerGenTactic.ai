class 'YnnariPowerGenTactic' (BaseTactic)

YnnariPowerGen = {}

function YnnariPowerGenTactic:__init( base_ai ) super( base_ai )

	self:SetName("Power Generator Tactic")

	-- Not used, not necessary
end

function YnnariPowerGenTactic:InitAbilities()

	if YnnariPowerGen.overload_id == nil then
		YnnariPowerGen.overload_id = cpu_manager.stats:GetAbilityID( "ynnari_power" )
	end
end

function YnnariPowerGenTactic:DoAbilities()

	if self.base_ai:CanDoAbility(YnnariPowerGen.overload_id) then
		self.base_ai:DoSpecialAbility(YnnariPowerGen.overload_id)
	end
end