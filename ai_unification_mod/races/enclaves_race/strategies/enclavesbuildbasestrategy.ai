					aitrace("BuildController: Dynamic build of "..tBuildType.name)
				end
			end
		end

		-- Force build commander squads to get immediate battlefield presence if research is build
		if (cpu_manager.cpu_player:IsResearchComplete("enclaves_ls_the_eight_shavastos_research")) then

			if (self:CountSquads("enclaves_squad_shavastos_ktgm") < 1) then

				-- Build the Commander Kill Team squads immediately
				local tBuildType = CpuBuildType()
				tBuildType.btype = CpuPrerequisites.BT_Squad
				tBuildType.name = "enclaves_squad_shavastos_ktgm"
				if (self:TryBuild( tBuildType )) then
					aitrace("BuildController: Dynamic build of "..tBuildType.name)
				end
			end
		end

		-- Force build commander squads to get immediate battlefield presence if research is build
		if (cpu_manager.cpu_player:IsResearchComplete("enclaves_ls_the_eight_torchstar_research")) then

			if (self:CountSquads("enclaves_squad_torchstar_ktgm") < 1) then

				-- Build the Commander Kill Team squads immediately
				local tBuildType = CpuBuildType()
				tBuildType.btype = CpuPrerequisites.BT_Squad
				tBuildType.name = "enclaves_squad_torchstar_ktgm"
				if (self:TryBuild( tBuildType )) then
					aitrace("BuildController: Dynamic build of "..tBuildType.name)
				end
			end
		end
	end

	-- Dynamic research item syntax: ResearchName, MinTier, RequisitionCost, PowerCost, MinSquadCap, MinSupportCap, SquadName, SquadMinCount
	local iArmyStrength = cpu_manager:GetArmyStrength()
	local iScoutSquads = self:CountSquads("enclaves_squad_patrol_team")
	local iInfantrySquads = self:CountSquads("enclaves_squad_strike_team") + self:CountSquads("enclaves_squad_breacher_team") +iScoutSquads
	local iHeroSquads = self:CountSquads("enclaves_squad_arrakon") + self:CountSquads("enclaves_squad_bravestorm") + self:CountSquads("enclaves_squad_brightsword") + self:CountSquads("enclaves_squad_farsight") + self:CountSquads("enclaves_squad_oblotai") + self:CountSquads("enclaves_squad_ovesa") + self:CountSquads("enclaves_squad_shavastos") + self:CountSquads("enclaves_squad_torchstar")

	-- Compute tier 1 researches
	if (self.tierLevel >= 1) then

		-- Determine if we research Orca Stealth
		if (self.m_iOrcaStealthResearchPattern == 1) then
			self:DynamicResearch("enclaves_stealth_orca_research", 1, 25, 25, 0, 0, nil, 0)
		else
			-- Skip research
		end
	end

	-- Compute tier 2 researches
	if (self.tierLevel >= 2) then
		self:DynamicResearch("enclaves_armour_orca_research", 2, 50, 50, 0, 0, nil, 0)

		-- Compute guevesa researches
		if (self:CountSquads("enclaves_squad_guevesa") + self:CountSquads("enclaves_squad_guevesa_ezo") >= 1) then 
			self:DynamicResearch("enclaves_upgrade_guardsmen_morale", 2, 75, 15, 0, 0, nil, 0)
			self:DynamicResearch("enclaves_upgrade_guardsmen_health", 2, 100, 50, 0, 0, nil, 0)
		end

		-- Compute secondary researches
		if (iArmyStrength >= 1000) then
			self:DynamicResearch("enclaves_research_fire", 2, 250, 100, 0, 0, nil, 0)
			self:DynamicResearch("enclaves_research_earth", 2, 250, 100, 0, 0, nil, 0)
		end

		-- Compute hero researches
		if (iHeroSquads >= 2) then
			self:DynamicResearch("enclaves_hero_health", 2, 200, 100, 0, 0, nil, 0)
			self:DynamicResearch("enclaves_hero_morale", 2, 100, 100, 0, 0, nil, 0)
		end

		-- Compute Ezo researches
		self:DynamicResearch("enclaves_research_ezo_reclaim", 2, 250, 100, 0, 0, nil, 0)

		-- Compute T2->T3 Research
		self:DynamicResearch("enclaves_tier2_research", 2, 400, 250, 0, 0, nil, 0)

		-- Try to build no-limit defensive turrets
		self:DynamicBuild("enclaves_turret_no_limit", 2, 2, 300, 200, 0, 0)
	end
	
	-- Compute tier 3 researches
	if (self.tierLevel >= 3) then

		-- Compute secondary researches
		if (iArmyStrength >= 1500) then
			self:DynamicResearch("enclaves_research_water", 3, 250, 100, 0, 0, nil, 0)
			self:DynamicResearch("enclaves_research_air", 3, 250, 100, 0, 0, nil, 0)
		end

		-- Compute hero researches
		if (iHeroSquads >= 2) then
			self:DynamicResearch("enclaves_hero_recruit", 3, 200, 100, 0, 0, nil, 0)
			self:DynamicResearch("enclaves_hero_wargear", 3, 150, 100, 0, 0, nil, 0)
		end

		-- Compute additional Hero Barracks based on available HQs
       		if self:GetBuildingCountByName(self:GetBuildingName("HQ")) > 1 then
			self:DynamicBuild("enclaves_herobarracks", 3, 3, 200, 0, 0, 0)
			self:DynamicBuild("enclaves_herobarracks", 4, 3, 200, 0, 0, 0)
		end

		-- Compute T3->T4 Research
		self:DynamicResearch("enclaves_tier3_research", 3, 500, 375, 0, 0, nil, 0)

		-- Try to build no-limit defensive turrets
		self:DynamicBuild("enclaves_turret_no_limit", 4, 3, 350, 250, 0, 0)
	end

	-- Compute tier 4 researches
	if (self.tierLevel >= 4) then

		-- Compute tier4 researches
		self:DynamicResearch("enclaves_relic_research", 4, 400, 400, 0, 0, nil, 0)

		-- Compute titan researches
		self:DynamicResearch("enclaves_titans_tier_ii", 4, 800, 800, 10, 10, nil, 0)

		-- Compute additional Hero Barracks based on available HQs
       		if self:GetBuildingCountByName(self:GetBuildingName("HQ")) > 2 then
			self:DynamicBuild("enclaves_herobarracks", 5, 4, 200, 0, 0, 0)
			self:DynamicBuild("enclaves_herobarracks", 6, 4, 300, 0, 0, 0)
			self:DynamicBuild("enclaves_herobarracks", 7, 4, 400, 0, 0, 0)
			self:DynamicBuild("enclaves_herobarracks", 8, 4, 500, 0, 0, 0)
		end

		-- Try to build no-limit defensive turrets
		self:DynamicBuild("enclaves_turret_no_limit", 10, 4, 400, 300, 0, 0)
	end

	-- Restrict dynamic builds to hard difficulty or higher
	if (CpuManager.AISettings.bMultiBuildings) then
	
		-- Dynamic buildings
		-- Item-Syntax: BuildingName, BuildingCount, MinTier, MinRequisition, MinPower, MinSquadCap, MinSupportCap
		self:DynamicBuild("enclaves_herobarracks", 2, 3, 300, 100, 0, 0)
		self:DynamicBuild("enclaves_herobarracks", 3, 4, 400, 200, 0, 0)
		self:DynamicBuild("enclaves_landingzone", 2, 3, 500, 600, 0, 0)
		self:DynamicBuild("enclaves_landingzone", 3, 4, 700, 700, 0, 0)
		self:DynamicBuild("enclaves_hq", 2, 3, 600, 300, 0, 0)
		self:DynamicBuild("enclaves_hq", 3, 4, 800, 500, 0, 0)
		self:DynamicBuild("enclaves_arka_cannon", 1, 4, 700, 325, 0, 0)
		self:DynamicBuild("enclaves_shield_generator", 1, 4, 700, 325, 0, 0)
	end
end

-- Force AI to give priority to re-build HQ if lost and none are present
function EnclavesBuildBaseStrategy:BuildFirstHQ()

	-- Check if a plan exists
	local iBuildingID = cpu_manager.stats:GetBuildingID(self:GetBuildingName("HQ"))
	if (self:PlanExists("Build Building Plan", iBuildingID)) then
		return
	end

	-- Check if any HQ are in production
	for oBuildChannel in build_manager:GetBuildChannelAIs() do

		-- Check building ID
		if (oBuildChannel:GetBlueprintID() == iBuildingID and not oBuildChannel:ConstructionDone()) then
			return
		end
	end
		
	-- Build the HQ
	local sHQName = self:GetBuildingName("HQ")
	local tBuildType = CpuBuildType()
	tBuildType.btype = CpuPrerequisites.BT_Building
	tBuildType.name = sHQName
	if (self:TryBuild( tBuildType )) then
		aitrace("BuildController: Dynamic build of "..tBuildType.name)
	end
	return
end

-- Force AI to give priority to recruit a builder unit if none are present
function EnclavesBuildBaseStrategy:BuildFirstBuilder(iBuilderName)

	if self:CountSquads(iBuilderName) < 1 then

		-- Recruit a builder squad
		local tBuildType = CpuBuildType()
		tBuildType.btype = CpuPrerequisites.BT_Squad
		tBuildType.name = iBuilderName
		if (self:TryBuild( tBuildType )) then
			aitrace("BuildController: Dynamic build of "..tBuildType.name)
		end
		return
	end
end

