function EnclavesBuildBaseStrategy:UpdateTierLevel()

	-- Reset tier level
	self.tierLevel = 1

	-- Check presence of T4 researches
	if (cpu_manager.cpu_player:IsResearchComplete("enclaves_tier3_research")) then
		self.tierLevel = 4
		return
	end

	-- Check presence of T3 researches
	if (cpu_manager.cpu_player:IsResearchComplete("enclaves_tier2_research")) then
		self.tierLevel = 3
		return
	end

	-- Check presence of T2 researches
	if (cpu_manager.cpu_player:IsResearchComplete("enclaves_tier1_research")) then
		self.tierLevel = 2
		return
	end
end

function EnclavesBuildBaseStrategy:BuildFlexible()

