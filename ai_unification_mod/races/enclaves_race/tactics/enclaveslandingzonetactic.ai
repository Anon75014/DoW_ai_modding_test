class 'EnclavesLandingZoneTactic' (BaseTactic)

EnclavesLandingZone = {}

function EnclavesLandingZoneTactic:__init( base_ai ) super( base_ai )

	self:SetName("Enclaves Landing Zone Tactic")

	self.m_iArmouryID = cpu_manager.stats:GetBuildingID("enclaves_generator")
end

function EnclavesLandingZoneTactic:CanDeepStrikeTroops()
	return (cpu_manager.player_stats:GetNumBasesByBPID(self.m_iArmouryID, true) > 0)
end

