class 'EnclavesKrootShaperTactic' (EnclavesInfantryTactic)

function EnclavesKrootShaperTactic:__init( squad_ai ) super( squad_ai )

	self:SetName("Enclaves Kroot Shaper Tactic")
end

function EnclavesKrootShaperTactic:Update()

	if (self:IsComplete()) then
		return
	end
	
	-- State machine
	if (not InfantryTactic.Update( self )) then
		return
	end
	
	-- Attach to squad
	self:TryAttachSquad( true, false, 1000, 100, nil )
end
 