class 'RavenGuardMarineHQTactic' (BaseTactic)

function RavenGuardMarineHQTactic:__init( base_ai ) super( base_ai )

	self:SetName("Raven Guard Marine Barracks Tactic")

	self.m_iRelayID = cpu_manager.stats:GetBuildingID("raven_guard_space_marine_orbital_relay")
end

function RavenGuardMarineHQTactic:CanDeepStrikeTroops()
	return (cpu_manager.player_stats:GetNumBasesByBPID(self.m_iRelayID, true) > 0)
end