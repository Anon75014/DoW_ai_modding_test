class 'RavenGuardStormRavenTactic' (RavenGuardSpaceMarineVehicleTactic)

RavenGuardStormRaven = {}

function RavenGuardStormRavenTactic:__init( squad_ai ) super( squad_ai )

	self:SetName("Raven Guard Storm Raven tactic")
end

function RavenGuardStormRavenTactic:DoAbilities()

	-- New code to unstuck units with pathing issues, that can jump.
	-- Call it with [true], only for multi-membered squads.
	self:SolveStuckCase(false)
end

function RavenGuardStormRavenTactic:Update()

	-- State machine
	if (not VehicleTactic.Update( self )) then
		return false
	end
end

