class 'RavenGuardMarineBuildBaseStrategy' (BuildBaseStrategy)

function RavenGuardMarineBuildBaseStrategy:__init( baseinfo ) super( baseinfo )

	-- Add detector units (Best first, worst last)
	self:AddDetectorUnit("raven_guard_space_marine_squad_librarian")
	self:AddDetectorUnit("raven_guard_space_marine_squad_skull_probe")

	CpuPrerequisites2.AddSpecialItem("0titan_building_enable", CpuPrerequisites.BT_Research)
	CpuPrerequisites2.AddSpecialItem("0titans_tier_ii_enable", CpuPrerequisites.BT_Research)
	CpuPrerequisites2.AddSpecialItem("0more_vanilla_squads_enable", CpuPrerequisites.BT_Research)
	CpuPrerequisites2.AddSpecialItem("raven_guard_space_marine_squad_clerg", CpuPrerequisites.BT_Squad)
end

function RavenGuardMarineBuildBaseStrategy:ChooseBuildProgram()

	-- Check build program count
	if (table.getn(self.info.BuildPrograms) ~= 4) then
		return BuildBaseStrategy.ChooseBuildProgram(self)
	end

	-- Get map size and closest enemy distance
	local sMapSize, iClosestEnemyDistance = self:GetMapSize()
		
	-- Set probabilities of the strategies according to the map size
	local iBuildProgram1	-- Basic strategy
	local iBuildProgram2	-- Marine rush
	local iBuildProgram3	-- Assault rush
	local iBuildProgram4	-- Vehicle rush
	if (sMapSize == "small" or iClosestEnemyDistance <= 70 or cpu_manager.iPlayerCount == 2) then
	
		iBuildProgram1 = 40
		iBuildProgram2 = 30
		iBuildProgram3 = 30
		iBuildProgram4 = 0
		
	elseif (sMapSize == "large" and cpu_manager.iPlayerCount >= 6) then
	
		iBuildProgram1 = 40
		iBuildProgram2 = 20
		iBuildProgram3 = 20
		iBuildProgram4 = 20
	else
	
		iBuildProgram1 = 40
		iBuildProgram2 = 30
		iBuildProgram3 = 30
		iBuildProgram4 = 0
	end
	
	-- Modify probabilities according to the closest enemy player
	local oEnemy = cpu_manager:FindClosestEnemyPlayer(false)
	local sEnemy = oEnemy:GetPlayerRaceName()		
	if (sEnemy == "ork_race") then
	
		iBuildProgram2 = iBuildProgram2 + 10
		iBuildProgram3 = iBuildProgram3 - 10

	elseif (sEnemy == "eldar_race" or sEnemy == "guard_race" or sEnemy == "tau_race" or sEnemy == "necron_race") then

		iBuildProgram1 = iBuildProgram1 - 10
		iBuildProgram2 = iBuildProgram2 - 10
		iBuildProgram3 = iBuildProgram3 + 20
	end
		
	-- Now choose a program
	iBuildProgram2 = iBuildProgram1 + iBuildProgram2
	iBuildProgram3 = iBuildProgram2 + iBuildProgram3
	iBuildProgram4 = iBuildProgram3 + iBuildProgram4
	local iRandom = math.random(1, 100)
	if (iRandom <= iBuildProgram1) then
		return 1
	elseif (iRandom <= iBuildProgram2) then
		return 2
	elseif (iRandom <= iBuildProgram3) then
		return 3
	elseif (iRandom <= iBuildProgram4) then
		return 4
	end
	return 1
end

function RavenGuardMarineBuildBaseStrategy:EvaluateSquadCap()

	-- Check squad cap
	if (self:CheckSquadCap(300, 0)) then
		
		local tBuildType = CpuBuildType()
		tBuildType.btype = CpuPrerequisites.BT_Research
		
		if (not cpu_manager.cpu_player:IsResearchComplete( "raven_guard_squad_cap_research" )) then
			tBuildType.name = "raven_guard_squad_cap_research"
		elseif not cpu_manager.cpu_player:IsResearchComplete( "raven_guard_squad_cap_research_1" ) then
			tBuildType.name = "raven_guard_squad_cap_research_1"
		else
			return
		end
		
		if (self:TryBuild( tBuildType )) then
			aitrace("BuildController: Dynamic research of "..tBuildType.name)
		end
	end
	
	-- Check support cap
	if (self:CheckSupportCap(150, 300)) then
		
		local tBuildType = CpuBuildType()
		tBuildType.btype = CpuPrerequisites.BT_Research
		
		if (not cpu_manager.cpu_player:IsResearchComplete( "raven_guard_support_cap_research" )) then
			tBuildType.name = "raven_guard_support_cap_research"
		elseif (not cpu_manager.cpu_player:IsResearchComplete( "raven_guard_support_cap_research_1" )) then
			tBuildType.name = "raven_guard_support_cap_research_1"
		elseif (not cpu_manager.cpu_player:IsResearchComplete( "raven_guard_support_cap_research_2" )) then
			tBuildType.name = "raven_guard_support_cap_research_2"
		elseif (not cpu_manager.cpu_player:IsResearchComplete( "raven_guard_support_cap_research_3" )) then
			tBuildType.name = "raven_guard_support_cap_research_3"
		else
			return
		end
		
		if (self:TryBuild( tBuildType )) then
			aitrace("BuildController: Dynamic research of "..tBuildType.name)
		end
	end
end

function RavenGuardMarineBuildBaseStrategy:GetBuildingName( sType )

	-- Return race specific object string
	if (sType == "HQ") then
		return "raven_guard_space_marine_hq"

	elseif (sType == "Generator") then
		return "raven_guard_space_marine_generator"
		
	elseif (sType == "BiggerGenerator") then
		return "raven_guard_space_marine_thermo_generator"
		
	elseif (sType == "VehicleBuilding") then
		return "raven_guard_space_marine_vehicle_building"
		
	elseif (sType == "ListeningPost") then
		return "raven_guard_space_marine_listening_post"
		
	elseif (sType == "Turret") then
		return "raven_guard_space_marine_turret_bolter"
		
	elseif (sType == "Mine") then
		return "raven_guard_space_marine_mine_field"
	end
	
	return nil
end

