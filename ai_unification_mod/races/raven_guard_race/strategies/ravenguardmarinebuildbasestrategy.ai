-- Arkhan 01.2006: Inherited method to check if an addon is a tier addon
function RavenGuardMarineBuildBaseStrategy:IsTierAddon( sName, iTargetTier )

	-- Check addon name and target tier
	if (sName == "raven_guard_space_marine_hq_addon_1" and iTargetTier == 2) then
		return true
	elseif (sName == "raven_guard_space_marine_hq_addon_2" and iTargetTier == 3) then
		return true
	end
	return false
end

