function RavenGuardMarineBuildBaseStrategy:UpdateTierLevel()

	-- Reset tier level
	self.tierLevel = 1
	
	-- Prepare
	local iHQAddon1ID = cpu_manager.stats:GetAddOnID("raven_guard_space_marine_hq_addon_1")
	local iHQAddon2ID = cpu_manager.stats:GetAddOnID("raven_guard_space_marine_hq_addon_2")
	local oStats = cpu_manager.stats:GetPlayerStatsFromID( cpu_manager.player_id )
	
	-- Check HQ's for addons
	for oBase in oStats:GetBases() do
	
		-- Check for valid building
		if (oBase:IsValid() and not oBase:IsListeningPost()) then

			-- Check for HQ addon 2
			if (oBase:HasAddOn(iHQAddon2ID)) then
			
				-- We still have the proper add-on
				if (cpu_manager.cpu_player:IsResearchComplete("raven_guard_marine_heavy_armor_deployment")) then
					self.tierLevel = 4
					return
				else
					self.tierLevel = 3
					return
				end
			
			-- Check for HQ addon 1
			elseif (oBase:HasAddOn(iHQAddon1ID)) then
				self.tierLevel = 2
			end
		end
	end
end

function RavenGuardMarineBuildBaseStrategy:BuildFlexible()

	-- Always test first to see if there is at least one existing HQ
	if self:GetBuildingCountByName(self:GetBuildingName("HQ")) < 1 then
     		self:BuildFirstHQ()
	end

	-- Always test first to see if there is at least one existing builder unit
	self:BuildFirstBuilder("raven_guard_space_marine_squad_techmarine")

	-- Dynamic research item syntax: ResearchName, MinTier, RequisitionCost, PowerCost, MinSquadCap, MinSupportCap, SquadName, SquadMinCount
	local iArmyStrength = cpu_manager:GetArmyStrength()
	local iLibrarianSquads	= self:CountSquads("raven_guard_space_marine_squad_librarian")
	local iCommanderSquads	= self:CountSquads("raven_guard_space_marine_squad_force_commander") + self:CountSquads("raven_guard_space_marine_squad_chaplain") + iLibrarianSquads
	local iInfantrySquads	= self:CountSquads("raven_guard_space_marine_squad_scout") + self:CountSquads("raven_guard_space_marine_squad_tactical") + self:CountSquads("raven_guard_space_marine_squad_assault") + self:CountSquads("raven_guard_space_marine_squad_sternguard_veteran") + self:CountSquads("raven_guard_space_marine_squad_vanguard_veteran") + self:CountSquads("raven_guard_space_marine_squad_eliminator")
	
	-- Compute tier 1 researches
	if (self.tierLevel >= 1) then

		-- Compute infantry researches
		if (iInfantrySquads >= 3) then
			self:DynamicResearch("raven_guard_frag_grenade_research", 1, 25, 75, 0, 0, "raven_guard_space_marine_squad_tactical", 2)
			self:DynamicResearch("raven_guard_infiltration_research", 1, 50, 50, 0, 0, nil, 0)
		end

		-- Compute secondary researches
		if (iArmyStrength >= 750) then

			-- Compute infantry researches
			if (iInfantrySquads >= 3) then
				self:DynamicResearch("raven_guard_marine_health_upgrade_research", 1, 50, 30, 0, 0, nil, 0)
				self:DynamicResearch("raven_guard_marine_accuracy_upgrade_research", 1, 75, 35, 0, 0, nil, 0)
			end
		end
		
		-- Compute bonus researches
		if (iArmyStrength >= 1250) then
		
			-- Compute infantry researches
			if (iInfantrySquads >= 3) then
				self:DynamicResearch("raven_guard_marine_sergeant_melee_upgrade_1", 1, 50, 20, 0, 0, nil, 0)
			end
		end
	end
	
	-- Compute tier 2 researches
	if (self.tierLevel >= 2) then
	
		-- Compute infantry researches
		if (iInfantrySquads >= 3) then
			self:DynamicResearch("raven_guard_marine_health_upgrade_research_2", 2, 100, 50, 0, 0, nil, 0)
			self:DynamicResearch("raven_guard_marine_sergeant_ranged_upgrade_1", 2, 50, 20, 0, 0, nil, 0)
		end
		
		-- Compute commander researches
		if (iLibrarianSquads >= 1) then
			self:DynamicResearch("raven_guard_force_commander_ability_research_1", 2, 80, 35, 0, 0, nil, 0)
			self:DynamicResearch("raven_guard_commander_health_research_1", 2, 100, 25, 0, 0, nil, 0)
			self:DynamicResearch("raven_guard_librarian_research_1", 2, 75, 40, 0, 0, "raven_guard_space_marine_squad_librarian", 1)
		end
		
		-- Compute secondary researches
		if (iArmyStrength >= 1250) then
		
			-- Compute infantry researches
			if (iInfantrySquads >= 3) then
				self:DynamicResearch("raven_guard_marine_accuracy_upgrade_research_2", 2, 100, 50, 0, 0, nil, 0)
				self:DynamicResearch("raven_guard_melta_bomb_research", 2, 75, 75, 0, 0, nil, 0)
			end
		end
		
		-- Compute bonus researches
		if (iArmyStrength >= 1750) then
					
			-- Compute infantry researches
			if (iInfantrySquads >= 3) then
				self:DynamicResearch("raven_guard_marine_max_weapons_research", 2, 75, 75, 0, 0, nil, 0)
				self:DynamicResearch("raven_guard_marine_sergeant_melee_upgrade_2", 2, 50, 20, 0, 0, nil, 0)
				self:DynamicResearch("raven_guard_infiltration_research_2", 2, 100, 100, 0, 0, nil, 0)
			end
		
			-- Compute vehicle researches
			self:DynamicResearch("raven_guard_smoke_launcher_research", 2, 75, 25, 0, 0, "raven_guard_space_marine_squad_rhino", 1)
		end

		-- Try to build no-limit defensive turrets
		self:DynamicBuild("raven_guard_space_marine_turret_bolter_no_limit", 2, 2, 200, 100, 0, 0)
	end
	
	-- Compute tier 3 researches
	if (self.tierLevel >= 3) then
	
		-- Compute commander researches
		if (iCommanderSquads >= 1) then
			self:DynamicResearch("raven_guard_librarian_research_2", 3, 40, 50, 0, 0, "raven_guard_space_marine_squad_librarian", 1)
		end

		-- Compute secondary researches
		if (iArmyStrength >= 1750) then

			-- Compute commander researches
			if (iCommanderSquads >= 1) then
				self:DynamicResearch("raven_guard_commander_health_research_2", 3, 125, 50, 0, 0, nil, 0)
				self:DynamicResearch("raven_guard_force_commander_research_2", 3, 100, 50, 0, 0, nil, 0)
			end

			if (iInfantrySquads >= 3) then
				self:DynamicResearch("raven_guard_sprint_research_2", 3, 150, 125, 0, 0, nil, 0)
			end
		end
		
		-- Compute bonus researches
		if (iArmyStrength >= 2250) then

			-- Compute terminator researches
			if (self:CountSquads("raven_guard_space_marine_squad_terminator") + self:CountSquads("raven_guard_space_marine_squad_centurion_assault") >= 2) then
				self:DynamicResearch("raven_guard_personalteleporter_research", 3, 75, 125, 0, 0, nil, 0)
			end
		end

		-- Force build additional generators in late game if we can with enough requisition and when low on power
                local iReq = resource_manager:GetResourceAmount():Get( ResourceAmount.RT_Requisition )
                local iPow = resource_manager:GetResourceAmount():Get( ResourceAmount.RT_Power )
                if iReq > 400 and iReq/iPow > 1.6 then

                	-- Check if a plan exists
                	local iBuildingID = cpu_manager.stats:GetBuildingID(self:GetBuildingName("Generator"))
                    	if (self:PlanExists("Build Building Plan", iBuildingID)) then
                       		return
                    	end

             		-- Check if any generators are in production
                    	for oBuildChannel in build_manager:GetBuildChannelAIs() do
                        	-- Check building ID
                        	if (oBuildChannel:GetBlueprintID() == iBuildingID and not oBuildChannel:ConstructionDone()) then
                            		return
                        	end
                    	end

             		-- Build a Generator
                    	local tBuildType = CpuBuildType()
                    	tBuildType.btype = CpuPrerequisites.BT_Building
                    	tBuildType.name = self:GetBuildingName("Generator")
                    	if (self:TryBuild( tBuildType )) then
               			aitrace("BuildController: Dynamic build of "..tBuildType.name)
			end
		end

		-- Try to build no-limit defensive turrets
		self:DynamicBuild("raven_guard_space_marine_turret_bolter_no_limit", 4, 3, 300, 200, 0, 0)
	end

	-- Compute tier 4 researches
	if (self.tierLevel >= 4) then

		-- Compute HQ Tier2 and Tier3 Addons for future HQs
       		if self:GetBuildingCountByName(self:GetBuildingName("HQ")) > 1 then
			self:DynamicAddon("raven_guard_space_marine_hq_addon_1", 100, 4, 500, 500, 0, 0, nil, nil, false)
			self:DynamicAddon("raven_guard_space_marine_hq_addon_2", 100, 4, 750, 750, 0, 0, nil, nil, false)
		end

		-- Try to build no-limit defensive turrets
		self:DynamicBuild("raven_guard_space_marine_turret_bolter_no_limit", 10, 4, 400, 300, 0, 0)
	end

	-- Restrict dynamic builds to hard difficulty or higher
	if (CpuManager.AISettings.bMultiBuildings) then
	
		-- Dynamic buildings
		-- Item-Syntax: BuildingName, BuildingCount, MinTier, MinRequisition, MinPower, MinSquadCap, MinSupportCap
		self:DynamicBuild("raven_guard_space_marine_barracks", 2, 3, 800, 0, 0, 0)
		self:DynamicBuild("raven_guard_space_marine_barracks", 3, 4, 1500, 0, 0, 0)
		self:DynamicBuild("raven_guard_space_marine_vehicle_building", 2, 3, 200, 600, 0, 0)
		self:DynamicBuild("raven_guard_space_marine_vehicle_building", 3, 4, 500, 1500, 0, 0)
		self:DynamicBuild("raven_guard_space_marine_hq", 2, 3, 700, 300, 0, 0)
		self:DynamicBuild("raven_guard_space_marine_hq", 3, 4, 1000, 600, 0, 0)
		self:DynamicBuild("raven_guard_space_marine_titans_building", 1, 4, 500, 500, 0, 0)
	end
end

-- Force AI to give priority to re-build HQ if lost and none are present
function RavenGuardMarineBuildBaseStrategy:BuildFirstHQ()

	-- Check if a plan exists
	local iBuildingID = cpu_manager.stats:GetBuildingID(self:GetBuildingName("HQ"))
	if (self:PlanExists("Build Building Plan", iBuildingID)) then
		return
	end

	-- Check if any HQ are in production
	for oBuildChannel in build_manager:GetBuildChannelAIs() do

		-- Check building ID
		if (oBuildChannel:GetBlueprintID() == iBuildingID and not oBuildChannel:ConstructionDone()) then
			return
		end
	end
		
	-- Build the HQ
	local sHQName = self:GetBuildingName("HQ")
	local tBuildType = CpuBuildType()
	tBuildType.btype = CpuPrerequisites.BT_Building
	tBuildType.name = sHQName
	if (self:TryBuild( tBuildType )) then
		aitrace("BuildController: Dynamic build of "..tBuildType.name)
	end
	return
end

-- Force AI to give priority to recruit a builder unit if none are present
function RavenGuardMarineBuildBaseStrategy:BuildFirstBuilder(iBuilderName)

	if self:CountSquads(iBuilderName) < 1 then

		-- Recruit a builder squad
		local tBuildType = CpuBuildType()
		tBuildType.btype = CpuPrerequisites.BT_Squad
		tBuildType.name = iBuilderName
		if (self:TryBuild( tBuildType )) then
			aitrace("BuildController: Dynamic build of "..tBuildType.name)
		end
		return
	end
end

-- Arkhan 01.2006: Method to check if force tech should be computed
function RavenGuardMarineBuildBaseStrategy:ForceTech()

	-- Check time
	if (g_iGMT < 60 * CpuManager.ForceTech.StartTier1) then
		return false
	end
	
	-- Check ressources
	local iRequisition = resource_manager:GetResourceAmount():Get( ResourceAmount.RT_Requisition )
	local iPower = resource_manager:GetResourceAmount():Get( ResourceAmount.RT_Power )

	-- Check tier
	local iTierLevel = self:GetTierLevel()
	if (iTierLevel == 1) then
	
		-- Check resources
		if (iRequisition > 600 and iPower > 200) then
			return false
		end
		return not self.m_bHQAddon1
		
	elseif (iTierLevel == 2) then

		-- Check current build program
		if (self.m_iCurrentBuildProgram == 2) then

			-- Check build channel for ability building
			if (self:GetBuildingCountByName("raven_guard_space_marine_ability_building", false) < 1) then
				return true
			end		
		else
		
			-- Check build channel for machine cult
			if (self:GetBuildingCountByName("raven_guard_space_marine_vehicle_building", false) < 1) then
				return true
			end
		end
		
		-- Check time
		if (g_iGMT > 60 * CpuManager.ForceTech.StartTier2 and (iRequisition < 600 or iPower < 600)) then
			return true
		end
		
	elseif (iTierLevel == 3) then	

		-- Check time
		if (g_iGMT > 60 * CpuManager.ForceTech.StartTier3 and (iRequisition < 600 or iPower < 600)) then
			return true
		end
	end
	return false
end

-- Arkhan 03.2006: Return placement type for buildings
function RavenGuardMarineBuildBaseStrategy:GetPlacementType(iBuildingID)
	
	-- Check building
	if (iBuildingID == cpu_manager.stats:GetBuildingID(self:GetBuildingName("HQ"))) then
        	local count = self:GetBuildingCountByName(self:GetBuildingName("HQ"), false)
		local friend = cpu_manager:FindClosestFriendPlayer()
		if friend == nil then
			-- Lone Player
			if count == 0 and not cpu_manager:HQThreat() then
				return "HQ"
			else
				return "Safeplace"
			end
		else
			-- Has allies
			if count > 0 or cpu_manager:HQThreat() then
				return "HQ"
			else
				return "Safeplace"
			end
		end
	elseif (iBuildingID == cpu_manager.stats:GetBuildingID("raven_guard_space_marine_barracks") or
			iBuildingID == cpu_manager.stats:GetBuildingID("raven_guard_space_marine_vehicle_building")) then
		return "Military"
	elseif (iBuildingID == cpu_manager.stats:GetBuildingID("raven_guard_space_marine_titans_building")) then
		return "Front1"
	elseif (cpu_manager:IsTurret(iBuildingID) or
			iBuildingID == cpu_manager.stats:GetBuildingID("raven_guard_space_marine_turret_bolter_no_limit")) then
		return "Front2"
	elseif (cpu_manager:IsMine(iBuildingID)) then
		return "Mine"
	elseif (cpu_manager:IsGenerator(iBuildingID) or
			iBuildingID == cpu_manager.stats:GetBuildingID("raven_guard_space_marine_armoury")) then
		return "BaseBack"
	end
	return "Basic"
end
-- Arkhan 03.2006: Inherited method to modify squad demand
function RavenGuardMarineBuildBaseStrategy:ModifySquadDemand(iUnitID)



	-- No more skullprobes then squads 
	if (iUnitID == cpu_manager.stats:GetSquadID("raven_guard_space_marine_squad_skull_probe")) then
		
		-- Check number of attachable squads 
		local skullprobe_attach = function ( squad_ai )
			return (squad_ai:GetTactic():GetUnitStrength() >= 250 and squad_ai:CanReceiveAttachment())
		end
		local num_attach = self:CountSquads("raven_guard_space_marine_squad_assault", skullprobe_attach) +
				   self:CountSquads("raven_guard_space_marine_squad_vanguard_veteran", skullprobe_attach)
--[[
				   self:CountSquads("raven_guard_space_marine_squad_scout", skullprobe_attach) +
				   self:CountSquads("raven_guard_space_marine_squad_tactical", skullprobe_attach) +
				   self:CountSquads("raven_guard_space_marine_squad_centurion_assault", skullprobe_attach) + 
				   self:CountSquads("raven_guard_space_marine_squad_terminator", skullprobe_attach) 
 ]]
 		-- Check skullprobe count
		local num_skuprobe = self:CountSquads("raven_guard_space_marine_squad_skull_probe")
		if (2 * num_skuprobe >= num_attach) then
			return 0
		end	
	end
	
	-- Only build rhinos if we've full support cap
	if (iUnitID == cpu_manager.stats:GetSquadID("raven_guard_space_marine_squad_rhino")) then
		
		-- Check army strength
		if (cpu_manager:GetArmyStrength() < 4000) then
			return 0
		end
	end

	return BuildBaseStrategy.ModifySquadDemand(self, iUnitID)
end

-- Arkhan 11.2006: Virtual method for checking out relic units
function RavenGuardMarineBuildBaseStrategy:RelicRequired(sName)

	-- Check name
	if (sName == "raven_guard_space_marine_squad_chapter_master")
	or (sName == "raven_guard_space_marine_squad_stormraven_fury")
	or (sName == "raven_guard_space_marine_squad_thunderhawk")
	or (sName == "raven_guard_space_marine_squad_warhound_titan") then
		return true
	end
	return false
end