class 'ArtisanTactic' (EngineerTactic)

Artisan = {}

function ArtisanTactic:__init( squad_ai ) super( squad_ai )

	self:SetName("Custodes Artisan Tactic")
end

function ArtisanTactic:IsAffectedByMorale()
	return false
end

function ArtisanTactic:InitAbilities()

	if Artisan.dismantle_id == nil then
	   Artisan.dismantle_id = cpu_manager.stats:GetAbilityID( "marines_dismantle" )	
     	end
end

function ArtisanTactic:DoAbilities()

        Ability.DoAbilityTargetEntity( self.squad_ai, Artisan.dismantle_id, Ability.EntityFilters.CloseBaseEntityEnemy, 1)
end


