class 'ThirteenthcomHQTactic' (BaseTactic)

function ThirteenthcomHQTactic:__init( base_ai ) super( base_ai )

	self:SetName("Thirteenthcom HQ Tactic")

	self.m_iArmouryID = cpu_manager.stats:GetBuildingID("thirteenthcom_armoury")
end

function ThirteenthcomHQTactic:CanDeepStrikeTroops()
	return (cpu_manager.player_stats:GetNumBasesByBPID(self.m_iArmouryID, true) > 3)
end
