class 'DeathGuardVehicleTactic' (VehicleTactic)

function DeathGuardVehicleTactic:__init( squad_ai ) super( squad_ai )

	self:SetName("Death Guard Vehicle Tactic")
end

function DeathGuardVehicleTactic:CheckVehicleDance(sName)

	if (sName == "death_guard_squad_rhino" or
		sName == "death_guard_squad_blight_drone" or
		sName == "death_guard_squad_dreadnought" or
		sName == "death_guard_squad_dreadnought_hellfire" or
		sName == "death_guard_squad_defiler" or
		sName == "death_guard_squad_whirlwind" or
		sName == "death_guard_squad_plagueburst_crawler" or
		sName == "death_guard_squad_plague_reaper" or
		sName == "death_guard_squad_plague_reaper_7th" or
		sName == "death_guard_squad_plague_tower" or
		sName == "death_guard_squad_predator" or
		sName == "death_guard_squad_land_raider" or
		sName == "death_guard_squad_land_raider_helios" or
		sName == "death_guard_squad_warhound_titan") then
		return true
	end
	return false
end
