class 'DeathGuardApothecaryTactic' (DeathGuardInfantryTactic)

DeathGuardApothecary = {}

function DeathGuardApothecaryTactic:__init( squad_ai ) super( squad_ai )

	self:SetName("Death Guard Apothecary Tactic")
end

function DeathGuardApothecaryTactic:IsAttacker()
	return self:IsCommanderAttacker()
end

function DeathGuardApothecaryTactic:IsDefender()
	return self:IsCommanderDefender()
end

function DeathGuardApothecaryTactic:Update()

	if self:IsComplete() then
      	return
   	end

	if not InfantryTactic.Update( self ) then
		return
	end

   	self:TryAttachSquad( false, false, 1000, 60, nil )
end
