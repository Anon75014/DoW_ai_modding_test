function FallenAngelsBuildBaseStrategy:ChooseBuildProgram()

	-- Check build program count
	if (table.getn(self.info.BuildPrograms) ~= 3) then
		return BuildBaseStrategy.ChooseBuildProgram(self)
	end

	-- Try to maintain the same strategy, on AI re-initiation (Save->Load, Change Players, etc.)
	-- First see if we have already completed a branch research

	-- Then, see if we are currently building a branch research
	-- Regrettably, cued researches cannot be detected, only those that are currently being built


	-- Set probabilities of the strategies according to the map size
	local iBuildProgram1 = 33	-- 
	local iBuildProgram2 = 34	-- 
	local iBuildProgram3 = 33	-- 
	
	-- Modify probabilities according to the closest enemy player
	local oEnemy = cpu_manager:FindClosestEnemyPlayer(false)
	local sEnemy = oEnemy:GetPlayerRaceName()		
	if (sEnemy == "ork_race" or sEnemy == "guard_race") then

		iBuildProgram1 = iBuildProgram1 + 20
		iBuildProgram2 = iBuildProgram2 - 20
		iBuildProgram3 = iBuildProgram3 - 10
		
	elseif (sEnemy == "chaos_marine_race" or sEnemy == "necron_race") then
	
		iBuildProgram1 = iBuildProgram1 - 20
		iBuildProgram2 = iBuildProgram2 + 20
		iBuildProgram3 = iBuildProgram3 + 10
	end
		
	-- Now choose a program
	iBuildProgram2 = iBuildProgram1 + iBuildProgram2
	iBuildProgram3 = iBuildProgram2 + iBuildProgram3
	local iRandom = math.random(1, 100)
	if (iRandom <= iBuildProgram1) then
		return 1
	elseif (iRandom <= iBuildProgram2) then
		return 2
	elseif (iRandom <= iBuildProgram3) then
		return 3
	end
	return math.random(1, 3)
end

function FallenAngelsBuildBaseStrategy:EvaluateSquadCap()

	-- Check squad cap
	if (self:CheckSquadCap(300, 200)) then
		
		-- Build the Banner Bearer squad
		local tBuildType = CpuBuildType()
		tBuildType.btype = CpuPrerequisites.BT_Squad
		tBuildType.name = "fa_squad_banner_bearer"
		if (self:TryBuild( tBuildType )) then
			aitrace("BuildController: Dynamic build of "..tBuildType.name)
		end
		return
	end

	-- Check support cap
	if (self:CheckSupportCap(300, 100)) then
	
		-- Check if a plan exists
		local iBuildingID = cpu_manager.stats:GetBuildingID("fa_vehicle_building")
		if (self:PlanExists("Build Building Plan", iBuildingID)) then
			return
		end
		
		-- Check if any vehicle buildings are in production
		for oBuildChannel in build_manager:GetBuildChannelAIs() do
	
			-- Check building ID
			if (oBuildChannel:GetBlueprintID() == iBuildingID and not oBuildChannel:ConstructionDone()) then
				return
			end
		end
		
		-- Build a vehicle building
		local tBuildType = CpuBuildType()
		tBuildType.btype = CpuPrerequisites.BT_Building
		tBuildType.name = "fa_vehicle_building"
		if (self:TryBuild( tBuildType )) then
			aitrace("BuildController: Dynamic build of "..tBuildType.name)
		end
		return

	elseif (self:CheckSupportCap(400, 200)) then

		local tBuildType = CpuBuildType()
		tBuildType.btype = CpuPrerequisites.BT_Research
		
		if (not cpu_manager.cpu_player:IsResearchComplete( "fallen_support_cap_research" )) then
			tBuildType.name = "fallen_support_cap_research"
		elseif (not cpu_manager.cpu_player:IsResearchComplete( "fallen_support_cap_research_1" )) then
			tBuildType.name = "fallen_support_cap_research_1"
		elseif (not cpu_manager.cpu_player:IsResearchComplete( "fallen_support_cap_research_2" )) then
			tBuildType.name = "fallen_support_cap_research_2"
		elseif (not cpu_manager.cpu_player:IsResearchComplete( "fallen_support_cap_research_3" )) then
			tBuildType.name = "fallen_support_cap_research_3"
		else
			return
		end
		
		if (self:TryBuild( tBuildType )) then
			aitrace("BuildController: Dynamic research of "..tBuildType.name)
		end
	end
end

function FallenAngelsBuildBaseStrategy:GetBuildingName( sType )

	-- Return race specific object string
	if (sType == "HQ") then
		return "fa_hq"

	elseif (sType == "Generator") then
		return "fa_generator"
		--return "fa_armoury"

	elseif (sType == "BiggerGenerator") then
		return "fa_thermo_generator"
		
	elseif (sType == "VehicleBuilding") then
		return "fa_vehicle_building"
		
	elseif (sType == "ListeningPost") then
		return "fa_listening_post"
		
	elseif (sType == "Turret") then
		return "fa_turret_bolter"

	elseif (sType == "Mine") then
		return "fa_mine_field"
	end
	return nil
end

function FallenAngelsBuildBaseStrategy:GetAddonBuilding( sType )

	if (sType == "fallen_list_post_addon_1") then
		return "fa_listening_post"

	elseif (sType == "fallen_list_post_addon_2") then
		return "fa_listening_post"

	elseif (sType == "fallen_turret_plasma_addon") then
		return "fa_turret_bolter"

	elseif (sType == "fallen_turret_las_addon") then
		return "fa_turret_bolter"

	elseif (sType == "fallen_turret_plasma_addon") then
		return "fa_turret_bolter_no_limit"

	elseif (sType == "fallen_turret_las_addon") then
		return "fa_turret_bolter_no_limit"
	end
	return nil
end

