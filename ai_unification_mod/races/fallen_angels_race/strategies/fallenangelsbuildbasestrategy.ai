function FallenAngelsBuildBaseStrategy:UpdateTierLevel()

	-- Reset tier level
	self.tierLevel = 1
	
	-- Prepare
 	local oStats = cpu_manager.stats:GetPlayerStatsFromID( cpu_manager.player_id )
	
	-- Check HQ's for addons
	for oBase in oStats:GetBases() do
	
		-- Check for valid building
		if (oBase:IsValid() and not oBase:IsListeningPost()) then

			-- Check for Tier4 researches
			if (cpu_manager.cpu_player:IsResearchComplete("fallenangels_heavy_armour_deployment")) then 
				self.tierLevel = 4
				return
			end

			-- Check for Tier3 researches
			if (cpu_manager.cpu_player:IsResearchComplete("fallen_tier2_research")) then
				self.tierLevel = 3
				return
			end

			-- Check for HQ addon 1
			if (cpu_manager.cpu_player:IsResearchComplete("fallen_tier1_research_hq_1")) then
				self.tierLevel = 2
				return
			end
		end
	end
end

function FallenAngelsBuildBaseStrategy:BuildFlexible()

	-- Always test first to see if there is at least one existing HQ
	if self:GetBuildingCountByName(self:GetBuildingName("HQ")) < 1 then
     		self:BuildFirstHQ()
	end

	-- Always test first to see if there is at least one existing builder unit
	self:BuildFirstBuilder("fa_squad_warpsmith")

	-- Dynamic research item syntax: ResearchName, MinTier, RequisitionCost, PowerCost, MinSquadCap, MinSupportCap, SquadName, SquadMinCount
	local iArmyStrength = cpu_manager:GetArmyStrength()
	local iInfantrySquads = self:CountSquads("fa_squad_tactical") + self:CountSquads("fa_squad_raptor") + self:CountSquads("fa_squad_havoc")
	local iSpecialInfantrySquads = self:CountSquads("fa_squad_havoc")
	local iTerminatorSquads = self:CountSquads("fa_squad_terminator") + self:CountSquads("fa_squad_terminator_assault") + self:CountSquads("fa_squad_obliterator")
	local iLibrarianSquads = self:CountSquads("fa_squad_sorcerer")
	local iChaplainSquads = self:CountSquads("fa_squad_chaplain")
	local iLordSquads = self:CountSquads("fa_squad_lord")
	local iCommanderSquads = self:CountSquads("fa_squad_lord") + self:CountSquads("fa_squad_chaplain") + self:CountSquads("fa_squad_sorcerer")
	local iVehicleSquads = self:CountSquads("fa_squad_rhino") + self:CountSquads("fa_squad_razorback") + self:CountSquads("fa_squad_hell_blade")

	-- Compute tier 1 researches and squad demands
	if (self.tierLevel >= 1) then

		-- Compute infantry researches
		if (iInfantrySquads >= 2) then
			self:DynamicResearch("fallen_frag_grenade_research", 1, 25, 75, 0, 0, nil, 0)
		end
	end

	-- Compute tier 2 researches
	if (self.tierLevel >= 2) then

		-- Compute bonus researches
		if (iArmyStrength >= 1250) then
		
			-- Compute infantry researches
			if (iInfantrySquads >= 3) then
				self:DynamicResearch("fallen_melta_bomb_research", 2, 75, 75, 0, 0, nil, 0)
				self:DynamicResearch("fallenangels_health_upgrade_research", 2, 100, 50, 0, 0, nil, 0)
				self:DynamicResearch("fallenangels_sergeant_melee_upgrade_1", 2, 50, 20, 0, 0, nil, 0)
				self:DynamicResearch("fallenangels_sergeant_ranged_upgrade_1", 2, 50, 20, 0, 0, nil, 0)
				self:DynamicResearch("fallen_max_weapons_research", 2, 75, 75, 0, 0, nil, 0)
				self:DynamicResearch("fallenangels_accuracy_upgrade_research", 2, 100, 50, 0, 0, nil, 0)
				self:DynamicResearch("fallenangels_health_upgrade_research_2", 2, 150, 100, 0, 0, nil, 0)
			end
		
			-- Compute support cap to promote vehicles early on but only if we have one already built
			if (iVehicleSquads >= 1) then
				self:DynamicResearch("fallen_support_cap_research", 2, 125, 0, 0, 0, nil, 0)
			end
		end

		-- Compute commander researches
		if (iCommanderSquads >= 1) then
			self:DynamicResearch("fallenangels_commander_health_research_1", 2, 100, 25, 0, 0, nil, 0)
		end

		-- Compute librarian researches
		if (iLibrarianSquads >= 1) then
			self:DynamicResearch("fallenangels_lord_research_1", 2, 60, 20, 0, 0, nil, 0)
		end

		-- Force build vehicle squads to get immediate battlefield presence
		if (self:CountSquads("fa_squad_razorback") < 1) then

			-- Build the Razorback squad
			local tBuildType = CpuBuildType()
			tBuildType.btype = CpuPrerequisites.BT_Squad
			tBuildType.name = "fa_squad_razorback"
			if (self:TryBuild( tBuildType )) then
				aitrace("BuildController: Dynamic build of "..tBuildType.name)
			end
		end

		-- Try to build no-limit defensive turrets
		self:DynamicBuild("fa_turret_bolter_no_limit", 2, 2, 200, 100, 0, 0)
	end
	
	-- Compute tier 3 researches
	if (self.tierLevel >= 3) then

		if (iLibrarianSquads >= 1) then
			self:DynamicResearch("fallenangels_lord_research_2", 3, 100, 25, 0, 0, nil, 0)
		end

		-- Compute post researches
		self:DynamicResearch("fallen_renegade_lp_research", 3, 150, 50, 0, 0, nil, 0)

		-- Compute infantry researches
		if (iInfantrySquads >= 3) then
			self:DynamicResearch("fallenangels_sergeant_melee_upgrade_2", 3, 50, 20, 0, 0, nil, 0)
		      --self:DynamicResearch("fallenangels_sergeant_ranged_upgrade_2", 3, 50, 20, 0, 0, nil, 0)
			self:DynamicResearch("fallenangels_accuracy_upgrade_research_2", 3, 150, 100, 0, 0, nil, 0)
			self:DynamicResearch("fallenangels_health_upgrade_research_2", 3, 150, 100, 0, 0, nil, 0)
		end

		-- Compute commander researches
		if (iCommanderSquads >= 1) then
			self:DynamicResearch("fallenangels_commander_health_research_2", 3, 125, 50, 0, 0, nil, 0)
		end

		-- Compute secondary researches
		if (iArmyStrength >= 1750) then

			-- Compute infantry researches
			if (iInfantrySquads >= 3) then
				self:DynamicResearch("fallenangels_accuracy_upgrade_research_3", 3, 150, 100, 0, 0, nil, 0)
				self:DynamicResearch("fallenangels_health_upgrade_research_3", 3, 150, 100, 0, 0, nil, 0)
			end

			-- Compute terminator researches
			if (iTerminatorSquads >= 1) then
				self:DynamicResearch("fallenangels_personalteleporter_research", 3, 75, 125, 0, 0, nil, 0)
			end

			-- Compute support cap to further promote more vehicles mid-game
			self:DynamicResearch("fallen_support_cap_research_1", 3, 150, 0, 0, 0, nil, 0)
		end

		-- Force build additional generators in late game if we can with enough requisition and when low on power
                local iReq = resource_manager:GetResourceAmount():Get( ResourceAmount.RT_Requisition )
                local iPow = resource_manager:GetResourceAmount():Get( ResourceAmount.RT_Power )
                if iReq > 400 and iReq/iPow > 1.6 then

                	-- Check if a plan exists
                	local iBuildingID = cpu_manager.stats:GetBuildingID(self:GetBuildingName("Generator"))
                    	if (self:PlanExists("Build Building Plan", iBuildingID)) then
                       		return
                    	end

             		-- Check if any generators are in production
                    	for oBuildChannel in build_manager:GetBuildChannelAIs() do
                        	-- Check building ID
                        	if (oBuildChannel:GetBlueprintID() == iBuildingID and not oBuildChannel:ConstructionDone()) then
                            		return
                        	end
                    	end

             		-- Build a Generator
                    	local tBuildType = CpuBuildType()
                    	tBuildType.btype = CpuPrerequisites.BT_Building
                    	tBuildType.name = self:GetBuildingName("Generator")
                    	if (self:TryBuild( tBuildType )) then
               			aitrace("BuildController: Dynamic build of "..tBuildType.name)
			end
		end

		-- Try to build no-limit defensive turrets
		self:DynamicBuild("fa_turret_bolter_no_limit", 4, 3, 300, 200, 0, 0)
	end

	-- Compute tier 4 researches
	if (self.tierLevel >= 4) then

		-- Computer relic research
		self:DynamicResearch("fallenangels_lost_relics_found", 4, 50, 20, 0, 0, nil, 0)

		-- Compute support cap to further promote far more vehicles late game
		self:DynamicResearch("fallen_support_cap_research_2", 4, 150, 0, 0, 0, nil, 0)
		self:DynamicResearch("fallen_support_cap_research_3", 4, 150, 0, 0, 0, nil, 0)

		-- Compute titan researches
		self:DynamicResearch("fallenangels_titans_tier_i", 4, 250, 250, 0, 0, nil, 0)
		self:DynamicResearch("fallenangels_titans_tier_ii", 4, 500, 500, 0, 0, nil, 0)

		-- Try to build no-limit defensive turrets
		self:DynamicBuild("fa_turret_bolter_no_limit", 10, 4, 400, 300, 0, 0)
	end

	local iRequisition = resource_manager:GetResourceAmount():Get( ResourceAmount.RT_Requisition )
	local iPower = resource_manager:GetResourceAmount():Get( ResourceAmount.RT_Power )

	-- After all is done, attempt building the optional turrets! And... Make some delays :)
	if math.random(1, 10) < 5 then
		local total_trts = self:GetBuildingCountByName("fa_turret_bolter", true)
		self:DynamicBuild("fa_turret_bolter", total_trts+1, 2, 200, 100, 0, 0)
	end

	-- Restrict dynamic builds to hard difficulty or higher
	if (CpuManager.AISettings.bMultiBuildings) then
	
		-- Dynamic buildings
		-- Item-Syntax: BuildingName, BuildingCount, MinTier, MinRequisition, MinPower, MinSquadCap, MinSupportCap
		self:DynamicBuild("fa_barracks", 2, 2, 800, 0, 0, 0)
		self:DynamicBuild("fa_barracks", 3, 4, 1000, 0, 0, 0)
		self:DynamicBuild("fa_vehicle_building", 2, 3, 200, 300, 0, 0)
		self:DynamicBuild("fa_vehicle_building", 3, 4, 500, 600, 0, 0)
		self:DynamicBuild("fa_hq", 2, 3, 700, 300, 0, 0)
		self:DynamicBuild("fa_hq", 3, 4, 1000, 600, 0, 0)
	end
end

-- Force AI to give priority to re-build HQ if lost and none are present
function FallenAngelsBuildBaseStrategy:BuildFirstHQ()

	-- Check if a plan exists
	local iBuildingID = cpu_manager.stats:GetBuildingID(self:GetBuildingName("HQ"))
	if (self:PlanExists("Build Building Plan", iBuildingID)) then
		return
	end

	-- Check if any HQ are in production
	for oBuildChannel in build_manager:GetBuildChannelAIs() do

		-- Check building ID
		if (oBuildChannel:GetBlueprintID() == iBuildingID and not oBuildChannel:ConstructionDone()) then
			return
		end
	end
		
	-- Build the HQ
	local sHQName = self:GetBuildingName("HQ")
	local tBuildType = CpuBuildType()
	tBuildType.btype = CpuPrerequisites.BT_Building
	tBuildType.name = sHQName
	if (self:TryBuild( tBuildType )) then
		aitrace("BuildController: Dynamic build of "..tBuildType.name)
	end
	return
end

-- Force AI to give priority to recruit a builder unit if none are present
function FallenAngelsBuildBaseStrategy:BuildFirstBuilder(iBuilderName)

	if self:CountSquads(iBuilderName) < 1 then

		-- Recruit a builder squad
		local tBuildType = CpuBuildType()
		tBuildType.btype = CpuPrerequisites.BT_Squad
		tBuildType.name = iBuilderName
		if (self:TryBuild( tBuildType )) then
			aitrace("BuildController: Dynamic build of "..tBuildType.name)
		end
		return
	end
end

-- Arkhan 01.2006: Method to check if force tech should be computed
function FallenAngelsBuildBaseStrategy:ForceTech()

	-- Check time
	if (g_iGMT < 60 * CpuManager.ForceTech.StartTier1) then
		return false
	end
	
	-- Check ressources
	local iRequisition = resource_manager:GetResourceAmount():Get( ResourceAmount.RT_Requisition )
	local iPower = resource_manager:GetResourceAmount():Get( ResourceAmount.RT_Power )

	-- Check tier
	local iTierLevel = self:GetTierLevel()
	if (iTierLevel == 1) then
	
		-- Check resources
		if (iRequisition > 500 and iPower > 200) then
			return false
		end
		return true
		
	elseif (iTierLevel == 2) then

		-- Check build channel for machine cult
		if (self:GetBuildingCountByName("fa_vehicle_building", false) < 1) then
			return true
		end
		
		-- Check time
		if (g_iGMT > 60 * CpuManager.ForceTech.StartTier2 and (iRequisition < 600 or iPower < 600)) then
			return true
		end
		
	elseif (iTierLevel == 3) then	

		-- Check time
		if (g_iGMT > 60 * CpuManager.ForceTech.StartTier3 and (iRequisition < 600 or iPower < 600)) then
			return true
		end
	end
	return false
end

-- Arkhan 03.2006: Return placement type for buildings
function FallenAngelsBuildBaseStrategy:GetPlacementType(iBuildingID)
	
	-- Check building
	if (iBuildingID == cpu_manager.stats:GetBuildingID(self:GetBuildingName("HQ"))) then
        	local count = self:GetBuildingCountByName(self:GetBuildingName("HQ"), false)
		local friend = cpu_manager:FindClosestFriendPlayer()
		if friend == nil then
			-- Lone Player
			if count == 0 and not cpu_manager:HQThreat() then
				return "HQ"
			else
				return "Safeplace"
			end
		else
			-- Has allies
			if count > 0 or cpu_manager:HQThreat() then
				return "HQ"
			else
				return "Safeplace"
			end
		end
	elseif (iBuildingID == cpu_manager.stats:GetBuildingID("fa_barracks") or
			iBuildingID == cpu_manager.stats:GetBuildingID("fa_vehicle_building")) then
		return "Military"
	elseif iBuildingID == cpu_manager.stats:GetBuildingID("fa_armoury") then
		if self:GetBuildingCountByName("fa_armoury", false) == 0 then
			return "BaseBack"
		else
			return "Military"
		end
	elseif iBuildingID == cpu_manager.stats:GetBuildingID("fa_orbital_relay") then
		if self:GetBuildingCountByName("fa_orbital_relay", false) == 0 then
			return "BaseBack"
		else
			return "Military"
		end
	elseif iBuildingID == cpu_manager.stats:GetBuildingID("fa_auspex") then
		if self:GetBuildingCountByName("fa_auspex", false) == 0 then
			return "BaseBack"
		else
			return "Military"
		end
	elseif iBuildingID == cpu_manager.stats:GetBuildingID("fa_generator") then
		return "BaseBack"
	elseif (cpu_manager:IsTurret(iBuildingID) or
			iBuildingID == cpu_manager.stats:GetBuildingID("fa_turret_bolter_no_limit")) then
		return "Front2"
	elseif (cpu_manager:IsMine(iBuildingID)) then
		return "Mine"
	end
	return "Basic"
end
-- Arkhan 03.2006: Inherited method to modify squad demand
function FallenAngelsBuildBaseStrategy:ModifySquadDemand(iUnitID)

	-- No more apothecaries then squads 
	if (iUnitID == cpu_manager.stats:GetSquadID("fa_squad_apothecary")) then

		-- Check number of attachable squads 
		local apoth_attach = function ( squad_ai )
			return (squad_ai:GetTactic():GetUnitStrength() >= 250 and squad_ai:CanReceiveAttachment())
		end
		local num_attach = self:CountSquads("fa_squad_tactical", apoth_attach) +
				   self:CountSquads("fa_squad_havoc", apoth_attach)

 		-- Check apothecary count
		local num_apothecary = self:CountSquads("fa_squad_apothecary")

		if (num_apothecary >= num_attach) then
			return 0
		end	
	end
	return BuildBaseStrategy.ModifySquadDemand(self, iUnitID)
end

