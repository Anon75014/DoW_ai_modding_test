class 'FallenAngelsBarracksTactic' (BaseTactic)

function FallenAngelsBarracksTactic:__init( base_ai ) super( base_ai )

	self:SetName("Fallen Angels Barracks Tactic")

	self.m_iArmouryID = cpu_manager.stats:GetBuildingID("fa_armoury")
end

function FallenAngelsBarracksTactic:CanDeepStrikeTroops()
	return (cpu_manager.player_stats:GetNumBasesByBPID(self.m_iArmouryID, true) > 0)
end