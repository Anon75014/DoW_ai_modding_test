class 'FallenAngelsHQTactic' (BaseTactic)

function FallenAngelsHQTactic:__init( base_ai ) super( base_ai )

	self:SetName("Fallen Angels HQ Tactic")

	self.m_iArmouryID = cpu_manager.stats:GetBuildingID("fa_armoury")
end

function FallenAngelsHQTactic:CanDeepStrikeTroops()
	return (cpu_manager.player_stats:GetNumBasesByBPID(self.m_iArmouryID, true) > 0)
end