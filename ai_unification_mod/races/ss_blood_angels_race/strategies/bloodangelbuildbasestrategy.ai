function BloodAngelBuildBaseStrategy:UpdateTierLevel()

	-- Reset tier level
	self.tierLevel = 1
	
	-- Prepare
	local iHQAddon1ID = cpu_manager.stats:GetAddOnID("blood_angel_hq_addon_1")
	local iHQAddon2ID = cpu_manager.stats:GetAddOnID("blood_angel_hq_addon_2")
	local oStats = cpu_manager.stats:GetPlayerStatsFromID( cpu_manager.player_id )
	
	-- Check HQ's for addons
	for oBase in oStats:GetBases() do

		-- Check for valid building
		if (oBase:IsValid() and not oBase:IsListeningPost()) then

		-- Check for HQ addon 2
		if (oBase:HasAddOn(iHQAddon2ID)) then
		
			-- We still have the proper add-on
			if (cpu_manager.cpu_player:IsResearchComplete("blood_angel_strength_of_primach_research")) then
				self.tierLevel = 4
				return
			else
				self.tierLevel = 3
				return
			end
		
		-- Check for HQ addon 1
		elseif (oBase:HasAddOn(iHQAddon1ID)) then
			self.tierLevel = 2
            end
        end
    end
end

function BloodAngelBuildBaseStrategy:BuildFlexible()

	-- Always test first to see if there is at least one existing HQ
	if self:GetBuildingCountByName(self:GetBuildingName("HQ")) < 1 then
     		self:BuildFirstHQ()
	end

	-- Always test first to see if there is at least one existing builder unit
	self:BuildFirstBuilder("blood_angel_squad_servitor")

	-- Dynamic research
	-- Item-Syntax: ResearchName, MinTier, RequisitionCost, PowerCost, MinSquadCap, MinSupportCap, SquadName, SquadMinCount
	local iArmyStrength = cpu_manager:GetArmyStrength()
	local num_infantry_squads = self:CountSquads("blood_angel_squad_tactical")+ self:CountSquads("blood_angel_squad_devastator")+ self:CountSquads("blood_angel_squad_assault")+ self:CountSquads("blood_angel_squad_assault_death_company")+ self:CountSquads("blood_angel_squad_veteran")+ self:CountSquads("blood_angel_squad_assault_veteran")
	local num_terminator_squads = self:CountSquads("blood_angel_squad_terminator")+ self:CountSquads("blood_angel_squad_terminator_assault")

	-- Compute tier 1 researches
	if (self.tierLevel >= 1) then
		
		-- Compute infantry researches
		if (num_infantry_squads >= 3) then
			self:DynamicResearch("blood_angel_frag_grenade_research", 1, 25, 75, 0, 0, nil, 0)
		end
		
		-- Compute bonus researches
		if (iArmyStrength >= 750) then
	
			-- Compute infantry researches
			if (num_infantry_squads >= 3) then
				self:DynamicResearch("blood_angel_sergeant_melee_upgrade_1", 1, 50, 20, 0, 0, nil, 0)
				self:DynamicResearch("blood_angel_sergeant_ranged_upgrade_1", 1, 50, 20, 0, 0, nil, 0)
			end
		end
	end

	-- Compute tier 2 researches
	if (self.tierLevel >= 2) then

		-- Compute secondary researches
		if (iArmyStrength >= 1250) then

			-- Compute infantry researches
			if (num_infantry_squads >= 3) then
				self:DynamicResearch("blood_angel_health_upgrade_research", 2, 160, 100, 0, 0, nil, 0)					
				self:DynamicResearch("blood_angel_heavy_weapons_research", 2, 75, 75, 0, 0, nil, 0)
			end

			-- Compute melta researches
			if (self:CountSquads("blood_angel_squad_assault") >= 1) then
				self:DynamicResearch("blood_angel_melta_bomb_research", 2, 75, 75, 0, 0, nil, 0)
			end
		
			-- Compute scout infiltrator researches
			if (self:CountSquads("blood_angel_squad_scout_assault") >= 1) then
				self:DynamicResearch("blood_angel_scout_infiltrate_research", 2, 100, 150, 0, 0, nil, 0)
			end

			-- Compute probe infiltrator researches
			if (self:CountSquads("blood_angel_squad_skull_probe") >= 1) then
				self:DynamicResearch("blood_angel_skull_probe_infiltrate_research", 2, 50, 50, 0, 0, nil, 0)
			end
		end

		-- Compute mephisto researches
		if (self:CountSquads("blood_angel_squad_hg_mephiston") >= 1) or (self:CountSquads("blood_angel_squad_mephiston") >= 1) then
			self:DynamicResearch("blood_angel_sword_research", 2, 70, 15, 0, 0, nil, 0)
		end

		-- Compute priest researches
		if (self:CountSquads("blood_angel_squad_highpriest") >= 1) then
			self:DynamicResearch("blood_angel_red_grail_research", 2, 60, 45, 0, 0, nil, 0)
		end

		-- Compute tycho researches
		if (self:CountSquads("blood_angel_squad_commander_tycho") >= 1) or (self:CountSquads("blood_angel_squad_commander_tycho_dc") >= 1) then
			self:DynamicResearch("blood_angel_commander_health_research_1", 2, 150, 0, 0, 0, nil, 0)
		end

		-- Compute rhino researches
		if (self:CountSquads("blood_angel_squad_rhino") >= 1) or (self:CountSquads("blood_angel_squad_death_company_rhino") >= 1) or (self:CountSquads("blood_angel_squad_damocles") >= 1) then
			self:DynamicResearch("blood_angel_smoke_launcher_research", 2, 75, 25, 0, 0, nil, 0)
		end

		-- Compute secondary researches
		if (iArmyStrength >= 1750) then
			
			-- Compute infantry researches
			if (num_infantry_squads >= 3) then
				self:DynamicResearch("blood_angel_sergeant_melee_upgrade_2", 2, 50, 20, 0, 0, nil, 0)
			end
		end

		-- Try to build no-limit defensive turrets
		self:DynamicBuild("blood_angel_turret_bolter_no_limit", 2, 2, 200, 100, 0, 0)
	end

	-- Compute tier 3 researches
	if (self.tierLevel >= 3) then

		-- Compute secondary researches
		if (iArmyStrength >= 1750) then

			-- Compute infantry researches
			if (num_infantry_squads >= 3) then
				self:DynamicResearch("blood_angel_accuracy_upgrade_research", 3, 100, 50, 0, 0, nil, 0)						
			end
		end

		if (self:CountSquads("blood_angel_squad_hg_mephiston") >= 1) or (self:CountSquads("blood_angel_squad_mephiston") >= 1) then
			self:DynamicResearch("blood_angel_rage_research", 3, 80, 50, 0, 0, nil, 0)
			self:DynamicResearch("blood_angel_quickening_research", 3, 80, 50, 0, 0, nil, 0)
		end

		-- Compute mephisto researches
		if (self:CountSquads("blood_angel_squad_hg_mephiston") >= 1) or (self:CountSquads("blood_angel_squad_mephiston") >= 1) then
			self:DynamicResearch("blood_angel_wings_research", 3, 50, 25, 0, 0, nil, 0)
		end

		-- Compute infantry researches
		if (num_infantry_squads >= 3) then
			self:DynamicResearch("blood_angel_terminator_honors_research", 3, 100, 25, 0, 0, nil, 0)
		end

		-- Compute tycho researches
		if (self:CountSquads("blood_angel_squad_commander_tycho") >= 1) or (self:CountSquads("blood_angel_squad_commander_tycho_dc") >= 1) then
			self:DynamicResearch("blood_angel_commander_health_research_2", 3, 225, 0, 0, 0, nil, 0)
		end
	
		-- Compute terminator researches
		if (num_terminator_squads >= 1) then
	     		self:DynamicResearch("blood_angel_personalteleporter_research", 3, 75, 125, 0, 0, nil, 0)
		end

		if (cpu_manager.cpu_player:IsResearchComplete("blood_angel_doctrine_might_of_sanguinus")) then

			-- Compute infantry researches
			if (num_infantry_squads >= 3) then
				self:DynamicResearch("blood_angel_sergeant_ranged_upgrade_2", 3, 60, 25, 0, 0, nil, 0)
			end
		end

		-- Force build additional generators in late game if we can with enough requisition and when low on power
                local iReq = resource_manager:GetResourceAmount():Get( ResourceAmount.RT_Requisition )
                local iPow = resource_manager:GetResourceAmount():Get( ResourceAmount.RT_Power )
                if iReq > 400 and iReq/iPow > 1.6 then

                	-- Check if a plan exists
                	local iBuildingID = cpu_manager.stats:GetBuildingID(self:GetBuildingName("Generator"))
                    	if (self:PlanExists("Build Building Plan", iBuildingID)) then
                       		return
                    	end

             		-- Check if any generators are in production
                    	for oBuildChannel in build_manager:GetBuildChannelAIs() do
                        	-- Check building ID
                        	if (oBuildChannel:GetBlueprintID() == iBuildingID and not oBuildChannel:ConstructionDone()) then
                            		return
                        	end
                    	end

             		-- Build a Generator
                    	local tBuildType = CpuBuildType()
                    	tBuildType.btype = CpuPrerequisites.BT_Building
                    	tBuildType.name = self:GetBuildingName("Generator")
                    	if (self:TryBuild( tBuildType )) then
               			aitrace("BuildController: Dynamic build of "..tBuildType.name)
			end
		end

		-- Try to build no-limit defensive turrets
		self:DynamicBuild("blood_angel_turret_bolter_no_limit", 4, 3, 300, 200, 0, 0)
	end

	-- Compute tier 4 researches
	if (self.tierLevel >= 4) then

		-- Computer tycho researches
		if (self:CountSquads("blood_angel_squad_commander_tycho") >= 1) or (self:CountSquads("blood_angel_squad_commander_tycho_dc") >= 1) then
			self:DynamicResearch("blood_angel_dc_tycho_research", 4, 100, 50, 0, 0, nil, 0)
		end

		-- Computer Titan researches
	     	self:DynamicResearch("blood_angel_titans_tier_i", 4, 400, 400, 0, 0, nil, 0)
	     	self:DynamicResearch("blood_angel_titans_tier_ii", 4, 800, 800, 0, 0, nil, 0)

		-- Compute HQ Tier2 and Tier3 Addons for future HQs
       		if self:GetBuildingCountByName(self:GetBuildingName("HQ")) > 1 then
			self:DynamicAddon("blood_angel_hq_addon_1", 100, 4, 500, 500, 0, 0, nil, nil, false)
			self:DynamicAddon("blood_angel_hq_addon_2", 50, 4, 1000, 1000, 0, 0, nil, nil, false)
		end

		-- Try to build no-limit defensive turrets
		self:DynamicBuild("blood_angel_turret_bolter_no_limit", 10, 4, 400, 300, 0, 0)
	end

	-- Restrict dynamic builds to hard difficulty or higher
	if (CpuManager.AISettings.bMultiBuildings) then
	
		-- Dynamic buildings
		-- Item-Syntax: BuildingName, BuildingCount, MinTier, MinRequisition, MinPower, MinSquadCap, MinSupportCap
		self:DynamicBuild("blood_angel_hq", 2, 3, 700, 100, 0, 0)
		self:DynamicBuild("blood_angel_hq", 3, 4, 1200, 200, 0, 0)
		self:DynamicBuild("blood_angel_barracks", 2, 3, 500, 100, 0, 0)
		self:DynamicBuild("blood_angel_barracks", 3, 4, 1000, 200, 0, 0)
		self:DynamicBuild("blood_angel_vehicle_building", 2, 3, 600, 100, 0, 0)
		self:DynamicBuild("blood_angel_vehicle_building", 3, 4, 900, 200, 0, 0)
		self:DynamicBuild("blood_angel_ability_building", 2, 3, 700, 100, 0, 0)
		self:DynamicBuild("blood_angel_ability_building", 3, 4, 900, 200, 0, 0)
	end
end

-- Force AI to give priority to re-build HQ if lost and none are present
function BloodAngelBuildBaseStrategy:BuildFirstHQ()

	-- Check if a plan exists
	local iBuildingID = cpu_manager.stats:GetBuildingID(self:GetBuildingName("HQ"))
	if (self:PlanExists("Build Building Plan", iBuildingID)) then
		return
	end

	-- Check if any HQ are in production
	for oBuildChannel in build_manager:GetBuildChannelAIs() do

		-- Check building ID
		if (oBuildChannel:GetBlueprintID() == iBuildingID and not oBuildChannel:ConstructionDone()) then
			return
		end
	end
		
	-- Build the HQ
	local sHQName = self:GetBuildingName("HQ")
	local tBuildType = CpuBuildType()
	tBuildType.btype = CpuPrerequisites.BT_Building
	tBuildType.name = sHQName
	if (self:TryBuild( tBuildType )) then
		aitrace("BuildController: Dynamic build of "..tBuildType.name)
	end
	return
end

-- Force AI to give priority to recruit a builder unit if none are present
function BloodAngelBuildBaseStrategy:BuildFirstBuilder(iBuilderName)

	if self:CountSquads(iBuilderName) < 1 then

		-- Recruit a builder squad
		local tBuildType = CpuBuildType()
		tBuildType.btype = CpuPrerequisites.BT_Squad
		tBuildType.name = iBuilderName
		if (self:TryBuild( tBuildType )) then
			aitrace("BuildController: Dynamic build of "..tBuildType.name)
		end
		return
	end
end

-- Arkhan 01.2006: Method to check if force tech should be computed
function BloodAngelBuildBaseStrategy:ForceTech()

	-- Check time
	if (g_iGMT < 60 * CpuManager.ForceTech.StartTier1) then
		return false
	end
	
	-- Check ressources
	local iRequisition = resource_manager:GetResourceAmount():Get( ResourceAmount.RT_Requisition )
	local iPower = resource_manager:GetResourceAmount():Get( ResourceAmount.RT_Power )

	-- Check tier
	local iTierLevel = self:GetTierLevel()
	if (iTierLevel == 1) then

		-- Check resources
		if (iRequisition > 600 and iPower > 200) then
			return false
		end
		return not self.m_bHQAddon1

	elseif (iTierLevel == 2) then

		-- Check build channel for machine cult
		iID = cpu_manager.stats:GetBuildingID("blood_angel_vehicle_building")
		local bHasVehicleBuilding = false
		for oBuildChannel in build_manager:GetBuildChannelAIs() do
		
			-- Check building ID
			if (oBuildChannel:GetBlueprintID() == iID) then
				bHasVehicleBuilding = true
				break
			end
		end
		if (not bHasVehicleBuilding) then
			return true
		end
		
		-- Check time
		if (g_iGMT > 60 * CpuManager.ForceTech.StartTier2 and (iRequisition < 600 or iPower < 600)) then
			return true
		end
		
	elseif (iTierLevel == 3) then	

		-- Check time
		if (g_iGMT > 60 * CpuManager.ForceTech.StartTier3 and (iRequisition < 600 or iPower < 600)) then
			return true
		end
	end
	return false
end

-- Arkhan 03.2006: Inherited method to modify squad demand
function BloodAngelBuildBaseStrategy:ModifySquadDemand(iUnitID)

	-- No more skullprobes then squads 
	if (iUnitID == cpu_manager.stats:GetSquadID("blood_angel_squad_skull_probe")) then
		
		-- Check number of attachable squads 
		local baskullprobe_attach = function ( squad_ai )
			return (squad_ai:GetTactic():GetUnitStrength() >= 250 and squad_ai:CanReceiveAttachment())
		end
		local num_attach = self:CountSquads("blood_angel_squad_scout_assault", baskullprobe_attach) +
				   self:CountSquads("blood_angel_squad_tactical", baskullprobe_attach) +
				   self:CountSquads("blood_angel_squad_assault", baskullprobe_attach) +
				   self:CountSquads("blood_angel_squad_veteran", baskullprobe_attach) + 
				   self:CountSquads("blood_angel_squad_devastator", baskullprobe_attach) + 
				   self:CountSquads("blood_angel_squad_terminator", baskullprobe_attach) + 
				   self:CountSquads("blood_angel_squad_terminator_assault", baskullprobe_attach) + 
				   self:CountSquads("blood_angel_squad_death_company", baskullprobe_attach) 
 
 		-- Check skullprobe count
		local num_baskullprobe = self:CountSquads("blood_angel_squad_skull_probe")
		if (2 * num_baskullprobe >= num_attach) then
			return 0
		end	
	end
	
	-- Only build rhinos if we've full support cap
	if (iUnitID == cpu_manager.stats:GetSquadID("blood_angel_squad_rhino")) then
		
		-- Check army strength
		if (cpu_manager:GetArmyStrength() < 4000) then
			return 0
		end
	end

	return BuildBaseStrategy.ModifySquadDemand(self, iUnitID)
end

-- Arkhan 03.2006: Return placement type for buildings
function BloodAngelBuildBaseStrategy:GetPlacementType(iBuildingID)
	
	-- Check building
	if (iBuildingID == cpu_manager.stats:GetBuildingID(self:GetBuildingName("HQ"))) then
        	local count = self:GetBuildingCountByName(self:GetBuildingName("HQ"), false)
		local friend = cpu_manager:FindClosestFriendPlayer()
		if friend == nil then
			-- Lone Player
                	if count == 0 and not cpu_manager:HQThreat() then
                    		return "HQ"
                	else
                    		return "Safeplace"
                	end
        	else
        		-- Has allies
                	if count > 0 or cpu_manager:HQThreat() then
 				return "HQ"
                	else
				return "Safeplace"
                	end
		end
	elseif (iBuildingID == cpu_manager.stats:GetBuildingID("blood_angel_barracks") or
			iBuildingID == cpu_manager.stats:GetBuildingID("blood_angel_vehicle_building") or
			iBuildingID == cpu_manager.stats:GetBuildingID("blood_angel_ability_building")) then
		return "Military"
	elseif (cpu_manager:IsTurret(iBuildingID) or
			iBuildingID == cpu_manager.stats:GetBuildingID("blood_angel_turret_bolter_no_limit")) then
		return "Front2"
	elseif (cpu_manager:IsMine(iBuildingID)) then
		return "Mine"
	elseif (cpu_manager:IsGenerator(iBuildingID) or
			iBuildingID == cpu_manager.stats:GetBuildingID("blood_angel_armoury") or
			iBuildingID == cpu_manager.stats:GetBuildingID("blood_angel_orbital_relay")) then
		return "BaseBack"
	end
	return "Basic"
end

