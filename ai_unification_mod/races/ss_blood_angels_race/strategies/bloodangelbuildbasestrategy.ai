class 'BloodAngelBuildBaseStrategy' (BuildBaseStrategy)

function BloodAngelBuildBaseStrategy:__init( baseinfo ) super( baseinfo )

	-- Add detector units (Best first, worst last)
	self:AddDetectorUnit("blood_angel_squad_skull_probe")
	self:AddDetectorUnit("blood_angel_squad_hg_mephiston")
	self:AddDetectorUnit("blood_angel_squad_hg_dante")
	self:AddDetectorUnit("blood_angel_squad_assault_veteran")
	self:AddDetectorUnit("blood_angel_squad_veteran")

	CpuPrerequisites2.AddSpecialItem("blood_angel_titans_tier_i", CpuPrerequisites.BT_Research)
	CpuPrerequisites2.AddSpecialItem("blood_angel_titans_tier_ii", CpuPrerequisites.BT_Research)
	CpuPrerequisites2.AddSpecialItem("blood_angel_squad_sicaran", CpuPrerequisites.BT_Squad)
	CpuPrerequisites2.AddSpecialItem("blood_angel_squad_sanguinor", CpuPrerequisites.BT_Squad)
	CpuPrerequisites2.AddSpecialItem("blood_angel_squad_land_raider_infernus", CpuPrerequisites.BT_Squad)
 	
	if (BuildBaseStrategy.ChooseBuildProgram(self) == 1) then
		g_iHarassingLeader = 1
   	else
		g_iHarassingLeader = 1
   	end
end

function BloodAngelBuildBaseStrategy:ChooseBuildProgram()

	-- Check build program count
	if (table.getn(self.info.BuildPrograms) ~= 2) then
		return BuildBaseStrategy.ChooseBuildProgram(self)
	end

	-- Try to maintain the same strategy, on AI re-initiation (Save->Load, Change Players, etc.)
	if cpu_manager.cpu_player:IsResearchComplete("blood_angel_doctrine_might_of_sanguinus") then
		return 1
	elseif cpu_manager.cpu_player:IsResearchComplete("blood_angel_doctrine_wings_of_sanguinus") then
		return 2
	end


	-- Get map size and closest enemy distance
	local sMapSize, iClosestEnemyDistance = self:GetMapSize()
		
	-- Set probabilities of the strategies according to the map size
	local iBuildProgram1	-- Might Of Sanguinius path
	local iBuildProgram2	-- Wings Of Sanguinius path
	if (sMapSize == "small" or iClosestEnemyDistance <= 70 or cpu_manager.iPlayerCount == 2) then
	
		iBuildProgram1 = 50
		iBuildProgram2 = 50
		
	elseif (sMapSize == "large" and cpu_manager.iPlayerCount >= 6) then
	
		iBuildProgram1 = 50
		iBuildProgram2 = 50
	else
	
		iBuildProgram1 = 50
		iBuildProgram2 = 50
	end
	
	-- Modify probabilities according to the closest enemy player
	local oEnemy = cpu_manager:FindClosestEnemyPlayer(false)
	local sEnemy = oEnemy:GetPlayerRaceName()		
	if (sEnemy == "ork_race" or sEnemy == "guard_race" or sEnemy == "necron_race") then
	
		iBuildProgram2 = iBuildProgram2 + 10

	elseif (sEnemy == "eldar_race" or sEnemy == "tau_race") then

		iBuildProgram1 = iBuildProgram1 - 10
		iBuildProgram2 = iBuildProgram2 - 10
	end
		
	-- Now choose a program
	iBuildProgram2 = iBuildProgram1 + iBuildProgram2
	local iRandom = math.random(1, 100)
	if (iRandom <= iBuildProgram1) then
		return 1
	elseif (iRandom <= iBuildProgram2) then
		return 2
	end
	return math.random(1,2)
end

function BloodAngelBuildBaseStrategy:EvaluateSquadCap()

	-- Check squad cap
	if (self:CheckSquadCap(300, 0)) then
		
		local tBuildType = CpuBuildType()
		tBuildType.btype = CpuPrerequisites.BT_Research
		
		if (not cpu_manager.cpu_player:IsResearchComplete( "blood_angel_squad_cap_research" )) then
			tBuildType.name = "blood_angel_squad_cap_research"
		elseif not cpu_manager.cpu_player:IsResearchComplete( "blood_angel_squad_cap_research_1" ) then
			tBuildType.name = "blood_angel_squad_cap_research_1"
		else
			return
		end
		
		if (self:TryBuild( tBuildType )) then
			aitrace("BuildController: Dynamic research of "..tBuildType.name)
		end
	end
	
	-- Check support cap
	if (self:CheckSupportCap(150, 200)) then
		
		local tBuildType = CpuBuildType()
		tBuildType.btype = CpuPrerequisites.BT_Research
		
		if (not cpu_manager.cpu_player:IsResearchComplete( "blood_angel_support_cap_research" )) then
			tBuildType.name = "blood_angel_support_cap_research"
		elseif (not cpu_manager.cpu_player:IsResearchComplete( "blood_angel_support_cap_research_1" )) then
			tBuildType.name = "blood_angel_support_cap_research_1"
		elseif (not cpu_manager.cpu_player:IsResearchComplete( "blood_angel_support_cap_research_2" )) then
			tBuildType.name = "blood_angel_support_cap_research_2"
		elseif (not cpu_manager.cpu_player:IsResearchComplete( "blood_angel_support_cap_research_3" )) then
			tBuildType.name = "blood_angel_support_cap_research_3"
		else
			return
		end
		
		if (self:TryBuild( tBuildType )) then
			aitrace("BuildController: Dynamic research of "..tBuildType.name)
		end
	end
end

function BloodAngelBuildBaseStrategy:GetBuildingName( sType )

	-- Return race specific object string
	if (sType == "HQ") then
		return "blood_angel_hq"

	elseif (sType == "Generator") then
		return "blood_angel_generator"
		
	elseif (sType == "BiggerGenerator") then
		return "blood_angel_thermo_generator"
		
	elseif (sType == "VehicleBuilding") then
		return "blood_angel_vehicle_building"
		
	elseif (sType == "ListeningPost") then
		return "blood_angel_listening_post"
		
	elseif (sType == "Turret") then
		return "blood_angel_turret_bolter"
		
	elseif (sType == "Mine") then
		return "blood_angel_mine_field"
	end
	
	return nil
end

function BloodAngelBuildBaseStrategy:GetAddonBuilding( sType )

	if (sType == "blood_angel_turret_addon") then
		return "blood_angel_turret_bolter"

	elseif (sType == "blood_angel_turret_addon") then
		return "blood_angel_turret_bolter_no_limit"

	elseif (sType == "blood_angel_list_post_addon_1") then
		return "blood_angel_listening_post"
		
	elseif (sType == "blood_angel_list_post_addon_2") then
		return "blood_angel_listening_post"
		
	elseif (sType == "blood_angel_hq_addon_1") then
		return "blood_angel_hq"
		
	elseif (sType == "blood_angel_hq_addon_2") then
		return "blood_angel_hq"
	end
	return nil
end

-- Arkhan 01.2006: Inherited method to check if an addon is a tier addon
function BloodAngelBuildBaseStrategy:IsTierAddon( sName, iTargetTier )

	-- Check addon name and target tier
	if (sName == "blood_angel_hq_addon_1" and iTargetTier == 2) then
		return true
	elseif (sName == "blood_angel_hq_addon_2" and iTargetTier == 3) then
		return true
	end
	return false
end

