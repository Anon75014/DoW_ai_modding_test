class 'DarkAngelsMasterRavenwingLandSpeederTactic' (DarkAngelsInfantryTactic)

DarkAngelsMasterRavenwingLandSpeeder = {}

function DarkAngelsMasterRavenwingLandSpeederTactic:__init( squad_ai ) super( squad_ai )

	self:SetName("Dark Angels Master Ravenwing Land Speeder Tactic")
end

function DarkAngelsMasterRavenwingLandSpeederTactic:IsAttacker()
	return self:IsCommanderAttacker()
end

function DarkAngelsMasterRavenwingLandSpeederTactic:IsDefender()
	return self:IsCommanderDefender()
end

function DarkAngelsMasterRavenwingLandSpeederTactic:InitAbilities()

end

function DarkAngelsMasterRavenwingLandSpeederTactic:DoAbilities()

    self:DoStrafingRun()
end

function DarkAngelsMasterRavenwingLandSpeederTactic:Update()

	if (self:IsComplete()) then
		return
	end
	
	-- State machine
	if (not InfantryTactic.Update(self)) then
		return
	end
end
