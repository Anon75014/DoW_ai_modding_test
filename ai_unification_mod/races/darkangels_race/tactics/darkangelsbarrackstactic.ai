class 'DarkAngelsBarracksTactic' (BaseTactic)

function DarkAngelsBarracksTactic:__init( base_ai ) super( base_ai )

	self:SetName("DarkAngels Barracks Tactic")

	self.m_iArmouryID = cpu_manager.stats:GetBuildingID("darkangels_armoury")
end

function DarkAngelsBarracksTactic:CanDeepStrikeTroops()
	return (cpu_manager.player_stats:GetNumBasesByBPID(self.m_iArmouryID, true) > 0)
end