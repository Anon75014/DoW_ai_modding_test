class 'templarbarrackstactic' (BaseTactic)

function templarbarrackstactic:__init( base_ai ) super( base_ai )

	self:SetName("Marine Barracks Tactic")

	self.m_iArmouryID = cpu_manager.stats:GetBuildingID("templar_armoury")
end

function templarbarrackstactic:CanDeepStrikeTroops()
	return (cpu_manager.player_stats:GetNumBasesByBPID(self.m_iArmouryID, true) > 0)
end