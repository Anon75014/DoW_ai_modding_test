class 'Renegades_Maiden_Tactic' (Renegades_Infantry_Tactic)

Maiden = {}

function Renegades_Maiden_Tactic:__init( squad_ai ) super( squad_ai )

	self:SetName("Renegades Maiden Tactic")

	-- Squad is able to occupy bunkers
	self.m_bBunkerSquad = true
end

function Renegades_Maiden_Tactic:IsAttacker()
	return self:IsCommanderAttacker()
end

function Renegades_Maiden_Tactic:IsDefender()
	return self:IsCommanderDefender()
end

function Renegades_Maiden_Tactic:Update()

	if (self:IsComplete()) then
		return
	end
	
	-- State machine
	if (not InfantryTactic.Update(self)) then
		return
	end
	
	-- Attach to squad
	self:TryAttachSquad(false, false, 1000, 60, nil)
end
 