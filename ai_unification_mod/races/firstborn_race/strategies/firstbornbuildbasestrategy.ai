function FirstbornBuildBaseStrategy:UpdateTierLevel()

	-- Reset tier level
	self.tierLevel = 1
	
	-- Prepare
	local iHQAddon1ID = cpu_manager.stats:GetAddOnID("addon_firstborn_hq_1")
	local iHQAddon2ID = cpu_manager.stats:GetAddOnID("addon_firstborn_hq_2")
	local oStats = cpu_manager.stats:GetPlayerStatsFromID( cpu_manager.player_id )

	-- Check HQ's for addons
	for oBase in oStats:GetBases() do
	
		-- Check for valid building
		if (oBase:IsValid() and not oBase:IsListeningPost()) then
	
		  	-- Check for HQ addon 2
			if (oBase:HasAddOn(iHQAddon2ID)) then
			
				-- We still have the proper add-on
				if (cpu_manager.cpu_player:IsResearchComplete("firstborn_upgrade_heavy_armour")) then
					self.tierLevel = 4
					return
				else
					self.tierLevel = 3
					return
				end
			
			-- Check for HQ addon 1 
			elseif (oBase:HasAddOn(iHQAddon1ID)) then
				self.tierLevel = 2
				return
			end
		end
	end
end

-- Check if we own a critical location
function FirstbornBuildBaseStrategy:HasCriticalLocation()

	-- Check all strategic points
	for oStrategicPoint in resource_manager:GetStrategicPointAIs() do
			
		-- Check strategic point
		if (oStrategicPoint:Owner() == cpu_manager.player_id and oStrategicPoint:IsStrategicObjective()) then
			return true
		end
	end
	return false
end

function FirstbornBuildBaseStrategy:BuildFlexible()

	-- Always test first to see if there is at least one existing HQ
	if self:GetBuildingCountByName(self:GetBuildingName("HQ")) < 1 then
     		self:BuildFirstHQ()
	end

	-- Always test first to see if there is at least one existing builder unit
	self:BuildFirstBuilder("firstborn_squad_enginseer")

	-- Dynamic research item syntax: ResearchName, MinTier, RequisitionCost, PowerCost, MinSquadCap, MinSupportCap, SquadName, SquadMinCount
	local iArmyStrength = cpu_manager:GetArmyStrength()
	local iBasicInfantrySquads = self:CountSquads("firstborn_squad_guardsmen") + self:CountSquads("firstborn_squad_veterans") + self:CountSquads("firstborn_squad_ambushers")
	local iCommanderSquads = self:CountSquads("firstborn_squad_commissar") + self:CountSquads("firstborn_squad_command_squad")  

	-- Compute tier 1 researches
	if (self.tierLevel >= 1) then
		
		-- Compute secondary researches
		if (iArmyStrength >= 750) then

			-- Compute infantry researches
			if (iBasicInfantrySquads >= 3) then
				self:DynamicResearch("firstborn_upgrade_long_knives", 1, 100, 50, 0, 0, nil, 0)
			end
		end
	end
	
	-- Compute tier 2 researches
	if (self.tierLevel >= 2) then

		-- Compute basic infantry researches
		if (iBasicInfantrySquads >= 3) then
			--self:DynamicResearch("firstborn_upgrade_sharpshooters", 2, 100, 100, 0, 0, nil, 0)
		end

		if (self:CountSquads("firstborn_squad_ambushers") >= 1) then
			self:DynamicResearch("firstborn_research_infiltration", 2, 150, 150, 0, 0, nil, 0)
		end

		-- Compute commander researches
		if (self:CountSquads("firstborn_squad_command_squad") >= 1) then
			self:DynamicResearch("firstborn_research_vostroyan_bravery", 1, 150, 150, 0, 0, nil, 0)
		end

		if (self:CountSquads("firstborn_squad_enginseer") >= 1) then
			self:DynamicResearch("firstborn_machine_healing_aura_research", 2, 100, 100, 0, 0, nil, 0)
		end

		-- Compute secondary researches
		if (iArmyStrength >= 1250) then
		
			-- Compute basic infantry researches
			if (iBasicInfantrySquads >= 3) then
				self:DynamicResearch("firstborn_upgrade_plasma_pistol", 2, 60, 50, 0, 0, nil, 0)
			end
		end

		-- Try to build no-limit defensive turrets or coils
		self:DynamicBuild("firstborn_turret_no_limit", 2, 2, 200, 150, 0, 0)
		self:DynamicBuild("firstborn_tesla_coil_no_limit", 2, 2, 300, 200, 0, 0)
	end
	
	-- Compute tier 3 researches
	if (self.tierLevel >= 3) then
	
		-- Compute misc and T3->T4 researches
		if (iArmyStrength >= 1250) then
			self:DynamicResearch("firstborn_research_volkite", 3, 60, 35, 0, 0, nil, 0)
			self:DynamicResearch("firstborn_upgrade_lasgun_relic", 3, 150, 150, 0, 0, nil, 0)
			self:DynamicResearch("firstborn_upgrade_heavy_armour", 3, 450, 450, 0, 0, nil, 0)
		end

		-- Force build additional generators in late game if we can with enough requisition and when low on power
                local iReq = resource_manager:GetResourceAmount():Get( ResourceAmount.RT_Requisition )
                local iPow = resource_manager:GetResourceAmount():Get( ResourceAmount.RT_Power )
                if iReq > 400 and iReq/iPow > 1.6 then

                	-- Check if a plan exists
                	local iBuildingID = cpu_manager.stats:GetBuildingID(self:GetBuildingName("Generator"))
                    	if (self:PlanExists("Build Building Plan", iBuildingID)) then
                       		return
                    	end

             		-- Check if any generators are in production
                    	for oBuildChannel in build_manager:GetBuildChannelAIs() do
                        	-- Check building ID
                        	if (oBuildChannel:GetBlueprintID() == iBuildingID and not oBuildChannel:ConstructionDone()) then
                            		return
                        	end
                    	end

             		-- Build a Generator
                    	local tBuildType = CpuBuildType()
                    	tBuildType.btype = CpuPrerequisites.BT_Building
                    	tBuildType.name = self:GetBuildingName("Generator")
                    	if (self:TryBuild( tBuildType )) then
               			aitrace("BuildController: Dynamic build of "..tBuildType.name)
			end
		end

		-- Try to build no-limit defensive turrets or coils
		self:DynamicBuild("firstborn_turret_no_limit", 4, 3, 300, 250, 0, 0)
		self:DynamicBuild("firstborn_tesla_coil_no_limit", 4, 3, 400, 300, 0, 0)
	end

	-- Compute tier 4 researches
	if (self.tierLevel >= 4) then

		-- Compute titan researches
		self:DynamicResearch("firstborn_upgrade_t1_titans", 4, 300, 300, 10, 10, nil, 0)
		self:DynamicResearch("firstborn_upgrade_t2_titans", 4, 450, 450, 10, 10, nil, 0)

		-- Compute black sun researches
		self:DynamicResearch("firstborn_research_project_black_sun", 4, 350, 350, 10, 10, nil, 0)

		-- Compute harmony researches
		self:DynamicResearch("firstborn_research_harmony", 4, 450, 450, 10, 10, nil, 0)

		if (self:GetBuildingCountByName("firstborn_mars_pattern") < 1) then

			-- Check if a plan exists
			local iBuildingID = cpu_manager.stats:GetBuildingID("firstborn_mars_pattern")
			if (self:PlanExists("Build Building Plan", iBuildingID)) then
				return
			end

			-- Check if any mars patterns are in production
			for oBuildChannel in build_manager:GetBuildChannelAIs() do
	
				-- Check building ID
				if (oBuildChannel:GetBlueprintID() == iBuildingID and not oBuildChannel:ConstructionDone()) then
					return
				end	
			end
		
			-- Build a Mars Pattern
			local tBuildType = CpuBuildType()
			tBuildType.btype = CpuPrerequisites.BT_Building
			tBuildType.name = "firstborn_mars_pattern"
			if (self:TryBuild( tBuildType )) then
				aitrace("BuildController: Dynamic build of "..tBuildType.name)
			end
		end

		if (self:GetBuildingCountByName("firstborn_grand_cannon") < 1) then

			-- Check if a plan exists
			local iBuildingID = cpu_manager.stats:GetBuildingID("firstborn_grand_cannon")
			if (self:PlanExists("Build Building Plan", iBuildingID)) then
				return
			end

			-- Check if any grand cannons are in production
			for oBuildChannel in build_manager:GetBuildChannelAIs() do
	
				-- Check building ID
				if (oBuildChannel:GetBlueprintID() == iBuildingID and not oBuildChannel:ConstructionDone()) then
					return
				end	
			end
		
			-- Build a Grand Cannon
			local tBuildType = CpuBuildType()
			tBuildType.btype = CpuPrerequisites.BT_Building
			tBuildType.name = "firstborn_grand_cannon"
			if (self:TryBuild( tBuildType )) then
				aitrace("BuildController: Dynamic build of "..tBuildType.name)
			end
		end

		-- Try to build Mars Pattern building addon if we have the correct prereqs
		if cpu_manager.cpu_player:IsResearchComplete("0liyue_enable") then

			if (self:GetBuildingCountByName("firstborn_mars_pattern") > 0) then
				self:DynamicAddon("addon_firstborn_tankdrop", 100, 4, 250, 250, 0, 0, nil, nil, false)
			end
		end

		-- Compute HQ Tier2/3 Addons and defensive buildings for future HQs
       		if self:GetBuildingCountByName(self:GetBuildingName("HQ")) > 1 then
			self:DynamicAddon("addon_firstborn_hq_1", 100, 4, 500, 500, 0, 0, nil, nil, false)
			self:DynamicAddon("addon_firstborn_hq_2", 50, 4, 1000, 1000, 0, 0, nil, nil, false)
  			self:DynamicBuild("firstborn_tesla_coil", 3, 4, 400, 300, 0, 0)
  			self:DynamicBuild("firstborn_turret", 9, 4, 300, 200, 0, 0)
		end

		-- Try to build no-limit defensive turrets or coils
		self:DynamicBuild("firstborn_turret_no_limit", 10, 4, 400, 350, 0, 0)
		self:DynamicBuild("firstborn_tesla_coil_no_limit", 10, 4, 500, 400, 0, 0)
	end

	-- Restrict dynamic builds to hard difficulty or higher
	if (CpuManager.AISettings.bMultiBuildings) then

		-- Dynamic buildings
		-- Item-Syntax: BuildingName, BuildingCount, MinTier, MinRequisition, MinPower, MinSquadCap, MinSupportCap
		self:DynamicBuild("firstborn_mars_pattern", 1, 4, 300, 100, 0, 0)
		self:DynamicBuild("firstborn_hq", 2, 3, 700, 50, 0, 0)
		self:DynamicBuild("firstborn_hq", 3, 4, 1000, 100, 0, 0)
		self:DynamicBuild("firstborn_barracks", 2, 3, 800, 0, 0, 0)
		self:DynamicBuild("firstborn_barracks", 2, 4, 2000, 0, 0, 0)
		self:DynamicBuild("firstborn_vehicle_factory", 2, 3, 200, 200, 0, 0)
		self:DynamicBuild("firstborn_vehicle_factory", 3, 4, 500, 300, 0, 0)
	end
end

-- Force AI to give priority to re-build HQ if lost and none are present
function FirstbornBuildBaseStrategy:BuildFirstHQ()

	-- Check if a plan exists
	local iBuildingID = cpu_manager.stats:GetBuildingID(self:GetBuildingName("HQ"))
	if (self:PlanExists("Build Building Plan", iBuildingID)) then
		return
	end

	-- Check if any HQ are in production
	for oBuildChannel in build_manager:GetBuildChannelAIs() do

		-- Check building ID
		if (oBuildChannel:GetBlueprintID() == iBuildingID and not oBuildChannel:ConstructionDone()) then
			return
		end
	end
		
	-- Build the HQ
	local sHQName = self:GetBuildingName("HQ")
	local tBuildType = CpuBuildType()
	tBuildType.btype = CpuPrerequisites.BT_Building
	tBuildType.name = sHQName
	if (self:TryBuild( tBuildType )) then
		aitrace("BuildController: Dynamic build of "..tBuildType.name)
	end
	return
end

-- Force AI to give priority to recruit a builder unit if none are present
function FirstbornBuildBaseStrategy:BuildFirstBuilder(iBuilderName)

	if self:CountSquads(iBuilderName) < 1 then

		-- Recruit a builder squad
		local tBuildType = CpuBuildType()
		tBuildType.btype = CpuPrerequisites.BT_Squad
		tBuildType.name = iBuilderName
		if (self:TryBuild( tBuildType )) then
			aitrace("BuildController: Dynamic build of "..tBuildType.name)
		end
		return
	end
end

-- Arkhan 01.2006: Method to check if force tech should be computed
function FirstbornBuildBaseStrategy:ForceTech()

	-- Check time
	if (g_iGMT < 60 * CpuManager.ForceTech.StartTier1) then
		return false
	end
	
	-- Check ressources
	local iRequisition = resource_manager:GetResourceAmount():Get( ResourceAmount.RT_Requisition )
	local iPower = resource_manager:GetResourceAmount():Get( ResourceAmount.RT_Power )

	-- Check tier
	local iTierLevel = self:GetTierLevel()
	if (iTierLevel == 1) then
	
		-- Check resources
		if (iRequisition > 600 and iPower > 200) then
			return false
		end
		return not self.m_bHQAddon1
		
	elseif (iTierLevel == 2) then

		-- Check for vehicle building
		if (self:GetBuildingCountByName("firstborn_vehicle_factory", false) < 1) then
			return true
		end
		
		-- Check time
		if (g_iGMT > 60 * CpuManager.ForceTech.StartTier2 and (iRequisition < 600 or iPower < 600)) then
			return true
		end
		
	elseif (iTierLevel == 3) then	

		-- Check time
		if (g_iGMT > 60 * CpuManager.ForceTech.StartTier3 and (iRequisition < 600 or iPower < 600)) then
			return true
		end
	end
	return false
end

-- Arkhan 03.2006: Return placement type for buildings
function FirstbornBuildBaseStrategy:GetPlacementType(iBuildingID)
	
	-- Check building
	if (iBuildingID == cpu_manager.stats:GetBuildingID(self:GetBuildingName("HQ"))) then
        	local count = self:GetBuildingCountByName(self:GetBuildingName("HQ"), false)
		local friend = cpu_manager:FindClosestFriendPlayer()
		if friend == nil then
			-- Lone Player
			if count == 0 and not cpu_manager:HQThreat() then
				return "HQ"
			else
				return "Safeplace"
			end
		else
			-- Has allies
			if count > 0 or cpu_manager:HQThreat() then
				return "HQ"
			else
				return "Safeplace"
			end
		end
	elseif (iBuildingID == cpu_manager.stats:GetBuildingID("firstborn_barracks") or
			iBuildingID == cpu_manager.stats:GetBuildingID("firstborn_vehicle_factory") or 
			iBuildingID == cpu_manager.stats:GetBuildingID("firstborn_regimental_command")) then 
		return "Military"
	elseif (iBuildingID == cpu_manager.stats:GetBuildingID("firstborn_mars_pattern")) then
		return "Front1"
	elseif (cpu_manager:IsTurret(iBuildingID) or
			iBuildingID == cpu_manager.stats:GetBuildingID("firstborn_turret_no_limit") or
			iBuildingID == cpu_manager.stats:GetBuildingID("firstborn_tesla_coil") or
			iBuildingID == cpu_manager.stats:GetBuildingID("firstborn_tesla_coil_no_limit")) then
		return "Front2"
	elseif (cpu_manager:IsGenerator(iBuildingID)) then
		return "BaseBack"
	end
	return "Basic"
end

-- Arkhan 03.2006: Inherited method to modify squad demand
function FirstbornBuildBaseStrategy:ModifySquadDemand(iUnitID)

	-- No more firstborn leaders than guard/kasrkin/ogryn squads
	if (iUnitID == cpu_manager.stats:GetSquadID("firstborn_squad_commissar") or
		iUnitID == cpu_manager.stats:GetSquadID("firstborn_squad_priest") or
		iUnitID == cpu_manager.stats:GetSquadID("firstborn_squad_medic")) then
			
		-- Check number of attachable squads
		local firstborn_attach = function( squad_ai )
			return (squad_ai:GetTactic():GetUnitStrength() >= 160 and squad_ai:CanReceiveAttachment())
		end
		local num_attach = self:CountSquads("firstborn_squad_guardsmen", firstborn_attach) + 
						   self:CountSquads("firstborn_squad_ambushers", firstborn_attach) +
						   self:CountSquads("firstborn_squad_veterans", firstborn_attach)
		-- Check number of leaders
		local num_leader = self:CountSquads("firstborn_squad_commissar") +
						   self:CountSquads("firstborn_squad_priest") +
						   self:CountSquads("firstborn_squad_medic")
		if (num_leader >= num_attach) then
			return 0
		end
	end

	return BuildBaseStrategy.ModifySquadDemand(self, iUnitID)
end

-- Arkhan 11.2006: Virtual method for checking out relic units
function FirstbornBuildBaseStrategy:RelicRequired(sName)

	-- Check name
	if (sName == "firstborn_squad_harazim") then
		return true
	elseif (sName == "firstborn_squad_mirror_maiden") then
		return true
	elseif (sName == "firstborn_squad_macharius") then
		return true
	elseif (sName == "firstborn_squad_praetor") then
		return true
	elseif (sName == "firstborn_squad_doomhammer") then
		return true
	end
	return false
end