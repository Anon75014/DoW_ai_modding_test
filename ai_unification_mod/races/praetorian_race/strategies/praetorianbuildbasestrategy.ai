function praetorianBuildBaseStrategy:UpdateTierLevel()

	-- Reset tier level
	self.tierLevel = 1

	-- Prepare
	local iHQAddon1ID = cpu_manager.stats:GetAddOnID("praetorian_hq_addon")
	local oStats = cpu_manager.stats:GetPlayerStatsFromID( cpu_manager.player_id )

	-- Check HQ's for addons
	for oBase in oStats:GetBases() do
	
		-- Check for valid building
		if (oBase:IsValid() and not oBase:IsListeningPost()) then
			-- Check for HQ addon 1 
			if (oBase:HasAddOn(iHQAddon1ID)) then
				if cpu_manager.cpu_player:IsResearchComplete("praetorian_full_scale_war") then
				self.tierLevel = 4
				return
			elseif self:GetBuildingCountByName("praetorian_labratory") > 1 then
				self.tierLevel = 3
				return
			end
			self.tierLevel = 2
			end
		end
	end
end

function praetorianBuildBaseStrategy:BuildFlexible()
  
	-- Always test first to see if there is at least one existing HQ
	if self:GetBuildingCountByName(self:GetBuildingName("HQ")) < 1 then
     		self:BuildFirstHQ()
	end

	-- Always test first to see if there is at least one existing builder unit
	self:BuildFirstBuilder("praetorian_squad_builder")

	-- Dynamic research item syntax: ResearchName, MinTier, RequisitionCost, PowerCost, MinSquadCap, MinSupportCap, SquadName, SquadMinCount
	local iArmyStrength = cpu_manager:GetArmyStrength()
	local iBasicInfantrySquads = self:CountSquads("praetorian_squad_praetorian_conscript") + self:CountSquads("praetorian_squad_praetorian_guardsmen") + self:CountSquads("praetorian_squad_team") + self:CountSquads("praetorian_squad_infantry_mortar") + self:CountSquads("praetorian_squad_infantry_missile")
	local iBasicOrksInfantrySquads = self:CountSquads("praetorian_squad_blood_axe") + self:CountSquads("praetorian_squad_orka_kommando")
	local iBasicOrksVehicleSquads = self:CountSquads("praetorian_squad_killa_kan") + self:CountSquads("praetorian_squad_burna_bomba") + self:CountSquads("praetorian_squad_ork_looted_tank")
	
	-- Compute tier 1 researches
	if (self.tierLevel >= 1) then
	
	end
	
	-- Compute tier 2 researches
	if (self.tierLevel >= 2) then

		-- Compute ork researches
		self:DynamicResearch("praetorian_ork_research_morechoppy", 2, 100, 50, 0, 0, "praetorian_squad_blood_axe", 1)

		-- Compute basic infantry researches
		if (iBasicInfantrySquads > 2) then
			self:DynamicResearch("praetorian_upgrade_guardsmen_health", 2, 100, 50, 0, 0, nil, 0)
			self:DynamicResearch("praetorian_guardsman_morale", 2, 75, 15, 0, 0, nil, 0)
		end
		
		-- Compute commander researches
		self:DynamicResearch("praetorian_research_command_squad_size", 2, 100, 100, 0, 0, nil, 0)

		-- Compute ork researches
		if (iBasicOrksInfantrySquads > 1) then
			self:DynamicResearch("praetorian_ork_research_eavy_armor_boyz", 2, 100, 100, 0, 0, nil, 0)
		end

		-- Compute secondary researches
		if (iArmyStrength >= 800) then
		
			-- Compute basic infantry researches
			if (iBasicInfantrySquads > 2) then
				self:DynamicResearch("praetorian_bayonets", 2, 50, 50, 0, 0, nil, 0)
				self:DynamicResearch("praetorian_guns", 2, 100, 100, 0, 0, nil, 0)
			end
		end	

		-- Try to build no-limit defensive turrets
		self:DynamicBuild("praetorian_turret_ork_no_limit", 2, 2, 200, 100, 0, 0)	
	end
	
	-- Compute tier 3 researches
	if (self.tierLevel >= 3) then
	
		-- Compute secondary researches
		if (iArmyStrength >= 1000) then
		
			if (iBasicInfantrySquads > 2) then
				self:DynamicResearch("praetorian_sergeant_melee_upgrade_1", 3, 50, 20, 0, 0, nil, 0)
				self:DynamicResearch("praetorian_sergeant_ranged_upgrade_1", 3, 50, 20, 0, 0, nil, 0)
				self:DynamicResearch("praetorian_guardsman_morale_2", 3, 75, 15, 0, 0, nil, 0)
				self:DynamicResearch("praetorian_upgrade_guardsmen_range", 3, 100, 25, 0, 0, nil, 0)
			end
		end

		-- Compute T3->T4 major research as AI sometimes fails to research
		if (iArmyStrength >= 2500) then
		
			-- Compute tier researches
			self:DynamicResearch("praetorian_full_scale_war", 3, 340, 340, 0, 0, nil, 0)
		end

		-- Try to build extra defensive turrets if additional HQs exist
       		if self:GetBuildingCountByName(self:GetBuildingName("HQ")) > 1 then
			self:DynamicBuild("praetorian_turret_ork", 6, 3, 600, 600, 0, 0)
		end

		-- Try to build extra defensive earthshakers if additional HQs exist
       		if self:GetBuildingCountByName(self:GetBuildingName("HQ")) > 1 then
			self:DynamicBuild("praetorian_turret_earthshaker", 4, 3, 200, 200, 0, 0)
		end

		-- Force build additional generators in late game if we can with enough requisition and when low on power
                local iReq = resource_manager:GetResourceAmount():Get( ResourceAmount.RT_Requisition )
                local iPow = resource_manager:GetResourceAmount():Get( ResourceAmount.RT_Power )
                if iReq > 400 and iReq/iPow > 1.6 then

                	-- Check if a plan exists
                	local iBuildingID = cpu_manager.stats:GetBuildingID(self:GetBuildingName("Generator"))
                    	if (self:PlanExists("Build Building Plan", iBuildingID)) then
                       		return
                    	end

             		-- Check if any generators are in production
                    	for oBuildChannel in build_manager:GetBuildChannelAIs() do
                        	-- Check building ID
                        	if (oBuildChannel:GetBlueprintID() == iBuildingID and not oBuildChannel:ConstructionDone()) then
                            		return
                        	end
                    	end

             		-- Build a Generator
                    	local tBuildType = CpuBuildType()
                    	tBuildType.btype = CpuPrerequisites.BT_Building
                    	tBuildType.name = self:GetBuildingName("Generator")
                    	if (self:TryBuild( tBuildType )) then
               			aitrace("BuildController: Dynamic build of "..tBuildType.name)
			end
		end

		-- Try to build no-limit defensive turrets
		self:DynamicBuild("praetorian_turret_ork_no_limit", 4, 3, 300, 200, 0, 0)
	end

	-- Compute tier 4 researches
	if (self.tierLevel >= 4) then

		-- Try to force build a Company Standard as sometimes the AI fails to build one
		if (self:GetBuildingCountByName("praetorian_company_standard") < 1) then

			-- Check if a plan exists
			local iBuildingID = cpu_manager.stats:GetBuildingID("praetorian_company_standard")
			if (self:PlanExists("Build Building Plan", iBuildingID)) then
				return
			end

			-- Check if any Company Standards are in production
			for oBuildChannel in build_manager:GetBuildChannelAIs() do
	
				-- Check building ID
				if (oBuildChannel:GetBlueprintID() == iBuildingID and not oBuildChannel:ConstructionDone()) then
					return
				end	
			end
		
			-- Build a Company Standard
			local tBuildType = CpuBuildType()
			tBuildType.btype = CpuPrerequisites.BT_Building
			tBuildType.name = "praetorian_company_standard"
			if (self:TryBuild( tBuildType )) then
				aitrace("BuildController: Dynamic build of "..tBuildType.name)
			end
		end

		-- Compute titan researches
		self:DynamicResearch("praetorian_titans_tier_i", 4, 500, 500, 0, 0, nil, 0)
		self:DynamicResearch("praetorian_titans_tier_ii", 4, 800, 800, 0, 0, nil, 0)

		-- Compute ork researches
		if (iBasicOrksVehicleSquads > 1) then
			self:DynamicResearch("praetorian_ork_research_extra_armour", 4, 150, 150, 0, 0, nil, 0)
		end

		-- Compute basic infantry researches
		if (iBasicInfantrySquads > 2) then
			self:DynamicResearch("praetorian_sergeant_melee_upgrade_2", 4, 50, 20, 0, 0, nil, 0)
		end
	
		-- Compute commander researches
		self:DynamicResearch("praetorian_armour_upgrade_research", 4, 100, 100, 0, 0, nil, 0)
	
		-- Compute ork researches
		self:DynamicResearch("praetorian_ork_research_evenmorechoppy", 4, 150, 100, 0, 0, "praetorian_squad_blood_axe", 1)

		-- Compute HQ Tier2 Addons for future HQs
       		if self:GetBuildingCountByName(self:GetBuildingName("HQ")) > 1 then
			self:DynamicAddon("praetorian_hq_addon", 100, 4, 500, 500, 0, 0, nil, nil, false)
		end

		-- Try to build extra defensive emplacements if additional HQs exist
       		if self:GetBuildingCountByName(self:GetBuildingName("HQ")) > 2 then
			self:DynamicBuild("praetorian_turret_earthshaker", 6, 4, 300, 300, 0, 0)
			self:DynamicBuild("praetorian_turret_ork", 8, 4, 700, 700, 0, 0)	
		end

		-- Try to build no-limit defensive turrets
		self:DynamicBuild("praetorian_turret_ork_no_limit", 10, 4, 400, 300, 0, 0)
	end

	-- Restrict dynamic builds to hard difficulty or higher
	if (CpuManager.AISettings.bMultiBuildings) then
	
		-- Dynamic buildings
		-- Item-Syntax: BuildingName, BuildingCount, MinTier, MinRequisition, MinPower, MinSquadCap, MinSupportCap
		self:DynamicBuild("praetorian_hq", 2, 3, 700, 300, 0, 0)
		self:DynamicBuild("praetorian_hq", 3, 4, 1000, 600, 0, 0)
		self:DynamicBuild("praetorian_mess", 2, 3, 600, 0, 0, 0)
		self:DynamicBuild("praetorian_mess", 3, 4, 700, 0, 0, 0)
		self:DynamicBuild("praetorian_facility", 2, 3, 200, 600, 0, 0)
		self:DynamicBuild("praetorian_facility", 3, 4, 400, 800, 0, 0)
		self:DynamicBuild("praetorian_aid_station", 2, 3, 1000, 0, 0, 0)
		self:DynamicBuild("praetorian_aid_station", 3, 4, 2000, 0, 0, 0)
		self:DynamicBuild("praetorian_orks", 2, 3, 300, 0, 0, 0)
		self:DynamicBuild("praetorian_orks", 3, 4, 500, 0, 0, 0)
	end
end

-- Force AI to give priority to re-build HQ if lost and none are present
function praetorianBuildBaseStrategy:BuildFirstHQ()

	-- Check if a plan exists
	local iBuildingID = cpu_manager.stats:GetBuildingID(self:GetBuildingName("HQ"))
	if (self:PlanExists("Build Building Plan", iBuildingID)) then
		return
	end

	-- Check if any HQ are in production
	for oBuildChannel in build_manager:GetBuildChannelAIs() do

		-- Check building ID
		if (oBuildChannel:GetBlueprintID() == iBuildingID and not oBuildChannel:ConstructionDone()) then
			return
		end
	end
		
	-- Build the HQ
	local sHQName = self:GetBuildingName("HQ")
	local tBuildType = CpuBuildType()
	tBuildType.btype = CpuPrerequisites.BT_Building
	tBuildType.name = sHQName
	if (self:TryBuild( tBuildType )) then
		aitrace("BuildController: Dynamic build of "..tBuildType.name)
	end
	return
end

-- Force AI to give priority to recruit a builder unit if none are present
function praetorianBuildBaseStrategy:BuildFirstBuilder(iBuilderName)

	if self:CountSquads(iBuilderName) < 1 then

		-- Recruit a builder squad
		local tBuildType = CpuBuildType()
		tBuildType.btype = CpuPrerequisites.BT_Squad
		tBuildType.name = iBuilderName
		if (self:TryBuild( tBuildType )) then
			aitrace("BuildController: Dynamic build of "..tBuildType.name)
		end
		return
	end
end

-- Arkhan 01.2006: Method to check if force tech should be computed
function praetorianBuildBaseStrategy:ForceTech()

	-- Check time
	if (g_iGMT < 60 * CpuManager.ForceTech.StartTier1) then
		return false
	end
	
	-- Check ressources
	local iRequisition = resource_manager:GetResourceAmount():Get( ResourceAmount.RT_Requisition )
	local iPower = resource_manager:GetResourceAmount():Get( ResourceAmount.RT_Power )

	-- Check tier
	local iTierLevel = self:GetTierLevel()
	if (iTierLevel == 1) then
	
		-- Check resources
		if (iRequisition > 600 and iPower > 200) then
			return false
		end
		return not self.m_bHQAddon1
		
	elseif (iTierLevel == 2) then

		-- Check for vehicle building
		if (self:GetBuildingCountByName("praetorian_labratory", false) == 0) then
			return true
		end
		
		-- Check time
		if (g_iGMT > 60 * CpuManager.ForceTech.StartTier2 and (iRequisition < 600 or iPower < 600)) then
			return true
		end
		
	elseif (iTierLevel == 3) then	

		-- Check time
		if (g_iGMT > 60 * CpuManager.ForceTech.StartTier3 and (iRequisition < 600 or iPower < 600)) then
			return true
		end
	end
	return false
end

-- Arkhan 03.2006: Return placement type for buildings
function praetorianBuildBaseStrategy:GetPlacementType(iBuildingID)
	
	-- Check building
	if (iBuildingID == cpu_manager.stats:GetBuildingID(self:GetBuildingName("HQ"))) then
        	local count = self:GetBuildingCountByName(self:GetBuildingName("HQ"), false)
		local friend = cpu_manager:FindClosestFriendPlayer()
		if friend == nil then
			-- Lone Player
			if count == 0 and not cpu_manager:HQThreat() then
				return "HQ"
			else
				return "Safeplace"
			end
		else
			-- Has allies
			if count > 0 or cpu_manager:HQThreat() then
				return "HQ"
			else
				return "Safeplace"
			end
		end
	elseif (iBuildingID == cpu_manager.stats:GetBuildingID("praetorian_mess") or
			iBuildingID == cpu_manager.stats:GetBuildingID("praetorian_labratory") or
			iBuildingID == cpu_manager.stats:GetBuildingID("praetorian_orks")) then
		return "Military"
	elseif (iBuildingID == cpu_manager.stats:GetBuildingID("praetorian_aid_station")) then
		return "Front1"
	elseif (cpu_manager:IsTurret(iBuildingID) or
			iBuildingID == cpu_manager.stats:GetBuildingID("praetorian_turret_ork") or
			iBuildingID == cpu_manager.stats:GetBuildingID("praetorian_turret_ork_no_limit")) then
		return "Front2"
	elseif (cpu_manager:IsGenerator(iBuildingID) or
			iBuildingID == cpu_manager.stats:GetBuildingID("praetorian_company_standard")) then
		return "BaseBack"
	end
	return "Basic"
end

-- Arkhan 03.2006: Inherited method to modify squad demand
function praetorianBuildBaseStrategy:ModifySquadDemand(iUnitID)

	-- No more guard leaders than squads
	if (iUnitID == cpu_manager.stats:GetSquadID("praetorian_squad_banner") or
		iUnitID == cpu_manager.stats:GetSquadID("praetorian_squad_bugler") or
		iUnitID == cpu_manager.stats:GetSquadID("praetorian_squad_commissar") or
		iUnitID == cpu_manager.stats:GetSquadID("praetorian_squad_medic")) then
			
		-- Check number of attachable squads
		local praetorian_attach = function( squad_ai )
			return (squad_ai:GetTactic():GetUnitStrength() >= 160 and squad_ai:CanReceiveAttachment())
		end
		local num_attach = self:CountSquads("praetorian_squad_praetorian_guardsmen", praetorian_attach) +
					self:CountSquads("praetorian_squad_praetorian_conscript", praetorian_attach)

		-- Check number of leaders
		local num_leader = self:CountSquads("praetorian_squad_banner") +
						   self:CountSquads("praetorian_squad_bugler") +
						   self:CountSquads("praetorian_squad_commissar") +
						   self:CountSquads("praetorian_squad_medic")
		if (num_leader >= num_attach) then
			return 0
		end
	end
	return BuildBaseStrategy.ModifySquadDemand(self, iUnitID)
end

-- Arkhan 11.2006: Virtual method for checking out relic units
function praetorianBuildBaseStrategy:RelicRequired(sName)

	-- Check name
	if (sName == "praetorian_squad_tardis") then
		return true
	elseif (sName == "praetorian_squad_ork_battlewagon") then
		return true
	elseif (sName == "praetorian_squad_shadowsword_titanicus") then
		return true
	elseif (sName == "praetorian_squad_stormhammer") then
		return true
	elseif (sName == "praetorian_squad_stormhammer_arkurian") then
		return true
	end
	return false
end