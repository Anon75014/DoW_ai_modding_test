class 'Praetorian_HQTactic' (BaseTactic)

function Praetorian_HQTactic:__init( base_ai ) super( base_ai )

	self:SetName("HQ Tactic")

	self.m_ilabID = cpu_manager.stats:GetBuildingID("praetorian_labratory")
end

function Praetorian_HQTactic:CanDeepStrikeTroops()
	return (cpu_manager.player_stats:GetNumBasesByBPID(self.m_ilabID, true) > 0)
end