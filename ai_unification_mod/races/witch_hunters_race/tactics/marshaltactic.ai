	return self:IsCommanderAttacker()
end

function MarshalTactic:IsDefender()
	return self:IsCommanderDefender()
end

function MarshalTactic:InitAbilities()

end

function MarshalTactic:DoAbilities()

	self:DoMarshalUseWeapon()
end
