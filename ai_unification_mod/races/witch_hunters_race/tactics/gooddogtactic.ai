class 'GoodDogTactic' (WitchHuntersInfantryTactic)

GoodDog = {}

function GoodDogTactic:__init( squad_ai ) super( squad_ai )

	self:SetName("Good Dog Tactic")
end

function GoodDogTactic:Update()

	if self:IsComplete() then
		return
	end
   
	--state machine
	if not InfantryTactic.Update( self ) then
		return
	end

   	--attach to squad
	self:TryAttachSquad( false, false, 1000, 200, nil )
end


