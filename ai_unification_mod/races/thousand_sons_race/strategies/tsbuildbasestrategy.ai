            		i_WG_Selection_number2 = 9	-- The number of researches to randomize, from the 2nd table

            		i_WG_Selections1 = {
                	{"thousand_sons_ls_ahriman_damage_recharge", 1, 150, 150, 0, 0, nil, 0},
                	{"thousand_sons_ls_ahriman_health_recharge", 1, 150, 150, 0, 0, nil, 0},
                	{"thousand_sons_ls_ahriman_inferno_bolts_1_research", 1, 150, 150, 0, 0, nil, 0},
                	{"thousand_sons_ls_ahriman_inferno_bolts_research", 1, 125, 125, 0, 0, nil, 0},
                	{"thousand_sons_ls_ahriman_massive_health", 1, 150, 150, 0, 0, nil, 0},
                	{"thousand_sons_ls_ahriman_morale", 1, 125, 125, 0, 0, nil, 0},
