			end
			-- Second, see if we can get a Ritual. Based on chance (33%)
			if  math.random(1, 3) == 1 then
				local ritualType = math.random(1, 3)
				if ritualType == 1 then
					AhrimanSpellArray[spellCounter] = "thousand_sons_ahriman_spell_05"
				elseif ritualType == 2 then
					AhrimanSpellArray[spellCounter] = "thousand_sons_ahriman_spell_06"
				else
					AhrimanSpellArray[spellCounter] = "thousand_sons_ahriman_spell_07"
				end
				spellCounter = spellCounter + 1
			end
			-- Third, fill with spells the remaining slots
			local slots = {0,0,0}
			for i=spellCounter, 3 do
				local done = false
				repeat
					local slot = math.random(1, 6)
					if slot == 1 then
						AhrimanSpellArray[i] = "thousand_sons_ahriman_spell_01"
					elseif slot == 2 then
						AhrimanSpellArray[i] = "thousand_sons_ahriman_spell_04"
					elseif slot == 3 then
						AhrimanSpellArray[i] = "thousand_sons_ahriman_spell_08"
					elseif slot == 4 then
						AhrimanSpellArray[i] = "thousand_sons_ahriman_spell_09"
					elseif slot == 5 then
						AhrimanSpellArray[i] = "thousand_sons_ahriman_spell_10"
					else
						AhrimanSpellArray[i] = "thousand_sons_ahriman_spell_11"
					end
					if i == 2 then
						if AhrimanSpellArray[2] ~= AhrimanSpellArray[1] then
							done = true
						end
					elseif i == 3 then
						if AhrimanSpellArray[3] ~= AhrimanSpellArray[2] and AhrimanSpellArray[3] ~= AhrimanSpellArray[1] then
							done = true
						end
					else
						done = true
					end
				until done
			end
		end
	else
		-- First entry has also the role of the one-time calculation "switch"
		AhrimanSpellArray[1] = ""
	end
	-- Done. Now, perform the 3 researches
	if self:CountSquads("thousand_sons_squad_ahriman") > 0 then
		self:DynamicResearch(AhrimanSpellArray[1], 1, 0, 0, 0, 0, nil, 0)
		self:DynamicResearch(AhrimanSpellArray[2], 1, 0, 0, 0, 0, nil, 0)
		self:DynamicResearch(AhrimanSpellArray[3], 1, 0, 0, 0, 0, nil, 0)
	end

	-- Chaos Last Standing Hero Wargear
	local iHeroLSCommanderSquads = self:CountSquads("thousand_sons_squad_ahriman_ktgm")
	if iHeroLSCommanderSquads > 0 then
		self:DynamicResearch("thousand_sons_ls_ahriman_lord_health_research_1", 1, 50, 50, 0, 0, nil, 0)
		self:DynamicResearch("thousand_sons_ls_ahriman_lord_health_research_2", 1, 75, 75, 0, 0, nil, 0)

		if not G_KTGM_Commander_WG_Choice_Made then
			 -- WARNING: Below, ALWAYS insert numbers <= of the total in each table!
           		 -- I used the actual total of each table, so that ALL will be randomly available
