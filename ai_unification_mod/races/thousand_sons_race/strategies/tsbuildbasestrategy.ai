function TSBuildBaseStrategy:UpdateTierLevel()

	-- Reset tier level
	self.tierLevel = 1
	
	-- Prepare
	local iHQAddon1ID = cpu_manager.stats:GetAddOnID("thousand_sons_hq_addon_1")
	local iHQAddon2ID = cpu_manager.stats:GetAddOnID("thousand_sons_hq_addon_2")
	local oStats = cpu_manager.stats:GetPlayerStatsFromID( cpu_manager.player_id )
	
	-- Check HQ's for addons
	for oBase in oStats:GetBases() do

		-- Check for valid building
		if (oBase:IsValid() and not oBase:IsListeningPost()) then

		-- Check for HQ addon 2
		if (oBase:HasAddOn(iHQAddon2ID)) then
		
			-- We still have the proper add-on
			if (cpu_manager.cpu_player:IsResearchComplete("thousand_sons_tier4")) then
				self.tierLevel = 4
				return
			else
				self.tierLevel = 3
				return
			end
		
		-- Check for HQ addon 1 
		elseif (oBase:HasAddOn(iHQAddon1ID)) then
			self.tierLevel = 2
		end
	end
    end
end

function TSBuildBaseStrategy:BuildFlexible()

	-- Always test first to see if there is at least one existing HQ
	if self:GetBuildingCountByName(self:GetBuildingName("HQ")) < 1 then
     		self:BuildFirstHQ()
	end

	-- Always test first to see if there is at least one existing builder unit
	self:BuildFirstBuilder("thousand_sons_squad_thrall")

	-- Sorcerer Lord's Spells
	-- Dynamic research item syntax: ResearchName, MinTier, RequisitionCost, PowerCost, MinSquadCap, MinSupportCap, SquadName, SquadMinCount
	if (self:CountSquads("thousand_sons_squad_lord") >= 1) then
		if (self.m_iSpellPathResearch == 1) then
		--Illusionist
			self:DynamicResearch("thousand_sons_path_illusionist_research", 1, 0, 0, 0, 0, nil, 0)
			self.m_iGiftsResearch = 1
		elseif (self.m_iSpellPathResearch == 2) then
		--Summoner
			self:DynamicResearch("thousand_sons_path_summoner_research", 1, 0, 0, 0, 0, nil, 0)
			self.m_iGiftsResearch = 2
		elseif (self.m_iSpellPathResearch == 3) then
		--Warlock
			self:DynamicResearch("thousand_sons_path_warlock_research", 1, 0, 0, 0, 0, nil, 0)
			self.m_iGiftsResearch = 3
		end
	end

	if ( cpu_manager.cpu_player:IsResearchComplete( "thousand_sons_path_illusionist_research" ) ) then
	--Illusionist
		if ( LordSpellResearchSelector <= 25 ) then
		--"cloner"
			if (self:CountSquads("thousand_sons_squad_lord") >= 1) then
			self:DynamicResearch("thousand_sons_spell_clone_lvl1_research", 1, 50, 50, 0, 0, nil, 0)
			self:DynamicResearch("thousand_sons_spell_invisibility_lvl1_research", 1, 50, 50, 0, 0, nil, 0)
			self:DynamicResearch("thousand_sons_spell_clone_lvl2_research", 2, 50, 50, 0, 0, nil, 0)
			self:DynamicResearch("thousand_sons_spell_doombolt_lord_lvl2a_research", 2, 50, 50, 0, 0, nil, 0)
			self:DynamicResearch("thousand_sons_spell_clone_lvl3_research", 3, 50, 50, 0, 0, nil, 0)
			end

		elseif ( LordSpellResearchSelector >= 26 and LordSpellResearchSelector <= 50 ) then
		--"cloaker"
			if (self:CountSquads("thousand_sons_squad_lord") >= 1) then
			self:DynamicResearch("thousand_sons_spell_invisibility_lvl1_research", 1, 50, 50, 0, 0, nil, 0)
			self:DynamicResearch("thousand_sons_spell_confusion_lvl1_research", 1, 50, 50, 0, 0, nil, 0)
			self:DynamicResearch("thousand_sons_spell_invisibility_lvl2_research", 2, 50, 50, 0, 0, nil, 0)
			self:DynamicResearch("thousand_sons_spell_doombolt_lord_lvl2a_research", 2, 50, 50, 0, 0, nil, 0)
			self:DynamicResearch("thousand_sons_spell_invisibility_lvl3_research", 3, 50, 50, 0, 0, nil, 0)

			end

		elseif ( LordSpellResearchSelector >= 51 and LordSpellResearchSelector <= 75 ) then
		--"confusionist"
			if (self:CountSquads("thousand_sons_squad_lord") >= 1) then
			self:DynamicResearch("thousand_sons_spell_confusion_lvl1_research", 1, 50, 50, 0, 0, nil, 0)
			self:DynamicResearch("thousand_sons_spell_clone_lvl1_research", 1, 50, 50, 0, 0, nil, 0)
