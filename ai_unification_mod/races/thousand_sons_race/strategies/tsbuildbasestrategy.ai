function TSBuildBaseStrategy:ChooseBuildProgram()
	-- NOTE: The actual strategy is chosen in tsstrategyinfo !! See there.
	-- Return a valid strategy on AI re-initiation
	if TSons_AI_StrategyChosen == 1 or TSons_AI_StrategyChosen == 2 then
		return TSons_AI_StrategyChosen
	else
		print("Something is wrong with the AI Strategy calculation!")
		TSons_AI_StrategyChosen = math.random(1,2)
		return TSons_AI_StrategyChosen
	end
end

function TSBuildBaseStrategy:EvaluateSquadCap()
	
	-- Check squad cap
	if (self:CheckSquadCap(300, 0)) then
		
		local tBuildType = CpuBuildType()
		tBuildType.btype = CpuPrerequisites.BT_Research
		
		if (not cpu_manager.cpu_player:IsResearchComplete( "thousand_sons_squad_cap_research" )) then
			tBuildType.name = "thousand_sons_squad_cap_research"
		elseif not cpu_manager.cpu_player:IsResearchComplete( "thousand_sons_squad_cap_research_1" ) then
			tBuildType.name = "thousand_sons_squad_cap_research_1"
		else
			return
		end
		
		if (self:TryBuild( tBuildType )) then
			aitrace("BuildController: Dynamic research of "..tBuildType.name)
		end
	end
		
	-- Check support cap
	if (self:CheckSupportCap(150, 300)) then
		
		local tBuildType = CpuBuildType()
		tBuildType.btype = CpuPrerequisites.BT_Research
		
		if (not cpu_manager.cpu_player:IsResearchComplete( "thousand_sons_support_cap_research" )) then
			tBuildType.name = "thousand_sons_support_cap_research"
		elseif (not cpu_manager.cpu_player:IsResearchComplete( "thousand_sons_support_cap_research_1" )) then
			tBuildType.name = "thousand_sons_support_cap_research_1"
		elseif (not cpu_manager.cpu_player:IsResearchComplete( "thousand_sons_support_cap_research_2" )) then
			tBuildType.name = "thousand_sons_support_cap_research_2"
		elseif (not cpu_manager.cpu_player:IsResearchComplete( "thousand_sons_support_cap_research_3" )) then
			tBuildType.name = "thousand_sons_support_cap_research_3"
		else
			return
		end
		
		if (self:TryBuild( tBuildType )) then
			aitrace("BuildController: Dynamic research of "..tBuildType.name)
		end
	end
end

function TSBuildBaseStrategy:GetBuildingName( sType )

	-- Return race specific object string
	if (sType == "HQ") then
		return "thousand_sons_hq"
		
	elseif (sType == "Generator") then
		return "thousand_sons_generator"
		
	elseif (sType == "BiggerGenerator") then
		return "thousand_sons_thermo_generator"
		
	elseif (sType == "VehicleBuilding") then
		return "thousand_sons_machine_altar"
		
	elseif (sType == "ListeningPost") then
		return "thousand_sons_listening_post"
		
	elseif (sType == "Turret") then
		return "thousand_sons_turret_bolter"
		
	elseif (sType == "Mine") then
		return "thousand_sons_mine_field"
	end
	
	return nil
end

function TSBuildBaseStrategy:GetAddonBuilding( sType )

	if ( sType == "thousand_sons_turret_addon" or sType == "thousand_sons_turret_invisibility_addon" ) then
		return "thousand_sons_turret_bolter"

	elseif ( sType == "thousand_sons_turret_addon" or sType == "thousand_sons_turret_invisibility_addon" ) then
		return "thousand_sons_turret_bolter_no_limit"

	elseif (sType == "thousand_sons_list_post_addon_1") then
		return "thousand_sons_listening_post"

	elseif (sType == "thousand_sons_list_post_addon_2") then
		return "thousand_sons_listening_post"

	elseif (sType == "thousand_sons_hq_addon_1") then
		return "thousand_sons_hq"

	elseif (sType == "thousand_sons_hq_addon_2") then
		return "thousand_sons_hq"
	end
	return nil
end

-- Arkhan 01.2006: Inherited method to check if an addon is a tier addon
function TSBuildBaseStrategy:IsTierAddon( sName, iTargetTier )

	-- Check addon name and target tier
	if (sName == "thousand_sons_hq_addon_1" and iTargetTier == 2) then
		return true
	elseif (sName == "thousand_sons_hq_addon_2" and iTargetTier == 3) then
		return true
	end
	return false
end

